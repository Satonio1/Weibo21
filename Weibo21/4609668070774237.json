[{"uid": "1618051664", "mid": "4609668070774237", "user_name": "头条新闻", "text": "【#新华社评吴孟达去世#：“达叔”带来的欢笑与思考将长留人间】日前，香港知名喜剧演员吴孟达因病离世，人们纷纷表达追思。人称“黄金配角”的“达叔”，塑造了不少经典角色，也写下了“我是中国人”的真情告白。“没吃过苦，就不会知道什么叫表演”。作为演员，敬业是本分，观众口碑就是最好的褒扬。作为中国人，爱国是本分，人人都有一份责任。斯人已逝，“达叔”带来的欢笑与思考留在人间……（新华视点）", "parent": null, "t": "1614513960"}, {"uid": "1618051664", "mid": "4609668682876660", "user_name": "头条新闻", "text": "吴孟达因肝癌去世，一文回顾生平>>O吴孟达因肝癌去世 一文回顾生平", "parent": "4609668070774237", "t": "1614514080"}, {"uid": "1618051664", "mid": "4609669143724668", "user_name": "头条新闻", "text": "吴孟达生前最后一条动态令人泪目>>O吴孟达节目中预感自己会走 最后动态令人泪目", "parent": "4609668070774237", "t": "1614514200"}, {"uid": "6373141466", "mid": "4609668683145500", "user_name": "HYH吴情", "text": "爱国者，人人都是主角！达叔已逝，欢笑经典永留人间！", "parent": "4609668070774237", "t": "1614514080"}, {"uid": "7358390415", "mid": "4609668511706096", "user_name": "陈大霄899", "text": "他不是配角，他和周星驰，就如郭德纲于谦，能说于谦是配角?", "parent": "4609668070774237", "t": "1614514020"}, {"uid": "2838082302", "mid": "4609668796391358", "user_name": "香烟·", "text": "三叔走好", "parent": "4609668070774237", "t": "1614514140"}, {"uid": "5706085047", "mid": "4609670016926206", "user_name": "炫酷的小陈simida", "text": "达叔一路走好，愿你在天堂就像给我们带来快乐的时候一样，天天开心", "parent": "4609668070774237", "t": "1614514380"}, {"uid": "6914593456", "mid": "4609668863496669", "user_name": "东先生V", "text": "论甘为绿叶奋斗不息的重要性", "parent": "4609668070774237", "t": "1614514140"}, {"uid": "7414695642", "mid": "4609685670070955", "user_name": "评NEWS", "text": "纯属闲的……", "parent": "4609668070774237", "t": "1614518160"}, {"uid": "2045633521", "mid": "4609740262605538", "user_name": "狂野塞伦盖蒂", "text": "撂下碗骂娘……", "parent": "4609685670070955", "t": "1614531180"}, {"uid": "6276811371", "mid": "4609717022230874", "user_name": "唯爱源源的小迷妹", "text": "你个喷子，看过达叔电影吗", "parent": "4609685670070955", "t": "1614525600"}, {"uid": "1787419133", "mid": "4609668163576298", "user_name": "济南同城会", "text": "小人物才生活", "parent": "4609668070774237", "t": "1614513960"}, {"uid": "72701875