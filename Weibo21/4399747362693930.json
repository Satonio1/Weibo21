[{"uid": "3045706314", "mid": "4399747362693930", "user_name": "杨春林情感美文", "text": "杨春林情感文库超话\n\n7月30日11:30左右，紫阳县东三岔路口发生山体崩塌，路边一辆SUV轿车和一辆面包车被落石砸中，事发后消防、公安、交警都已前往营救。\n\n据初步了解，事故导致一人当场死亡，两人受伤，其中一辆被埋汽车中疑似还有被困人员，目前救援工作正紧张进行。道路已进行交通管制，请车辆提前绕行", "parent": null, "t": "1564464960"}, {"uid": "1043896374", "mid": "4401767317988129", "user_name": "皇子Johnny", "text": "微博需要互动，好朋友需要走动！", "parent": "4399747362693930", "t": "1564946520"}, {"uid": "6587511627", "mid": "4400145569921322", "user_name": "挽回-情感-思念-修复", "text": "好微博是博主用心的杰作！", "parent": "4399747362693930", "t": "1564559880"}, {"uid": "6587511627", "mid": "4399915477790583", "user_name": "挽回-情感-思念-修复", "text": "微博要经常互动，是金子总会放光的。", "parent": "4399747362693930", "t": "1564505040"}, {"uid": "6962467481", "mid": "4399885929891922", "user_name": "情感修复屋", "text": "如果害怕失去就放弃拥有的权利，那么人生也就失去了所有的意义!", "parent": "4399747362693930", "t": "1564497960"}, {"uid": "6412950861", "mid": "4399857000810575", "user_name": "怎麼挽回前男友前任", "text": "他们都在用力的爱，却不懂得爱，大家都只知道我想要，我想要，可成熟的爱不是索要，而是给予。", "parent": "4399747362693930", "t": "1564491060"}, {"uid": "1742482691", "mid": "4399768136760206", "user_name": "叶艾茂", "text": "支持支持！", "parent": "4399747362693930", "t": "1564469880"}, {"uid": "6512286418", "mid": "4399750306471500", "user_name": "繁华落尽不落城", "text": "紫阳官方公众号“紫阳发布”发的消息：经核实，截止目前，山体垮塌约150方，砸中停在下面的两辆汽车。车内4人已全部救出，并送往县医院救治。目前1人伤势较重，正在全力抢救，详细情况续报。不知道你从哪儿听的有人死亡，未经核实的事情就敢这样在网上发布？", "parent": "4399747362693930", "t": "1564465620"}, {"uid": "5792488202", "mid": "4401241927443090", "user_name": "浩野户外旅行", "text": "值得欣赏，为你点赞", "parent": "4399747362693930", "t": "1564821300"}, {"uid": "5792488202", "mid": "4400894059711191", "user_name": "浩野户外旅行", "text": "无条件支持老友，留下足迹，常互动", "parent": "4399747362693930", "t": "1564738320"}]