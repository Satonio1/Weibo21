[{"uid": "1618051664", "mid": "4608870502371101", "user_name": "头条新闻", "text": "【#男子给家人通报病情坐医院痛哭#】2月24日，河南郑州。一男子在医院楼道给家里打电话放声痛哭，视频拍摄者岳先生称当时带老人去打针，中途接电话在楼道看到男子给家里打电话说病情，情绪比较失控，感觉世事无常，成年人的生活很不易。网友纷纷留言加油。@沸点视频 L沸点壹刻的微博视频", "parent": null, "t": "1614323760"}, {"uid": "5164972775", "mid": "4608871169523897", "user_name": "虾饺standby", "text": "去肿瘤医院看看，每天都有人哭肿眼睛的。被病情打败被治疗费打败比比皆是", "parent": "4608870502371101", "t": "1614323940"}, {"uid": "5721564003", "mid": "4609033257877602", "user_name": "居里夫人不是梦", "text": "确实……12年前爸爸生病住院 当时是单人病房区域 在隔壁病房 听说是一个年轻的女子癌症 那晚走了 他丈夫在过道哭到不能自已……开了房门望了过去 内心无比难受", "parent": "4608871169523897", "t": "1614362580"}, {"uid": "3985189398", "mid": "4608948160432676", "user_name": "在下云逸", "text": "现在生不起病，怕自己生病，怕家里人生病", "parent": "4608871169523897", "t": "1614342300"}, {"uid": "2822604965", "mid": "4608946248091123", "user_name": "阿婆爱波洛", "text": "回复@缝个书包上学去:加油，会好起来的。", "parent": "4608875192126483", "t": "1614341820"}, {"uid": "1736563705", "mid": "4608906287911519", "user_name": "___总是问我什么时候去接你", "text": "我妈妈在肿瘤医院住院的时候，印象最深的就是很多小孩子，头发都剃光了，在轮椅上坐着，看着真难受。", "parent": "4608871169523897", "t": "1614332340"}, {"uid": "6144155613", "mid": "4608893873291988", "user_name": "谁信星座谁傻比", "text": "有时候就在想，中年男人，上有老下有小，假设有一套按揭房，然后父母之一得了治不好的癌症，卖房的钱能延续三五年的寿命，这时候，他该怎么做呢", "parent": "4608871169523897", "t": "1614329340"}, {"uid": "1691116181", "mid": "4608881193649621", "user_name": "哑巴兔子", "text": "深有体会", "parent": "4608871169523897", "t": "1614326340"}, {"uid": "5233347455", "mid": "4608880443654338", "user_name": "八面威风杀气骠", "text": "确实，晚上有人哭的很大声，撕心裂肺的。我陪我爸看病的时候。那时候真的感觉不生病就是最好的", "parent": "4608871169523897", "t": "1614326160"}, {"uid": "7549799025", "mid": "4608875192126483", "user_name": "缝个书包上学去", "text": "确实，太难了，正在生病中，跑了几家医院了，病情不好转😭", "parent": "4608871169523897", "t": "1614324900"}, {"uid": "6816163940", "mid": "4608873745615272", "user_name": "Prado307", "text": "的确是", "parent": "4608871169523897", "t": "1614324540"}, {"uid": "7118300168", "mid": "4608872578547892", "user_name": "流浪人诚诚666", "text": "唉", "parent": "4608871169523897", "t": "1614324300"}, {"uid": "7108620125", "mid": "4608872033026202", "user_name": "跑步大毛", "text": "确实是", "parent": "4608871169523897", "t": "1614324180"}, {"uid": "5957836097", "mid": "4608871123389328", "user_name": "财神姐", "text": "去医院看到太多世间冷暖辛酸，普通人太不容易了", "parent": "4608870502371101", "t": "1614323940"}, {"uid": "1854127855", "mid": "4608875237742626", "user_name": "四分局鸡舍", "text": "@头条新闻 你们天天一整发这玩意（路人甲拍路人乙）都经过当事人允许了吗？即使公共场合的是不是也得给被拍者做点技术处理，声音或者影像的。这点职业素养和公德不懂吗？用得着你们天天当社会情感指挥棒和心灵导师了？", "parent": "4608870502371101", "t": "1614324900"}, {"uid": "2809912833", "mid": "4608870797018850", "user_name": "小小小小22222222", "text": "希望每个人都能身体健康平安。", "parent": "4608870502371101", "t": "1614323880"}, {"uid": "2494175321", "mid": "4608899422094464", "user_name": "呜呼呜呼·", "text": "一起健康哦", "parent": "4608870797018850", "t": "1614330660"}, {"uid": "6388920102", "mid": "4608879709128948", "user_name": "YDYoHoo", "text": "你也是哦", "parent": "4608870797018850", "t": "1614325980"}, {"uid": "6190236455", "mid": "4608878949702024", "user_name": "钻石VS岩石", "text": "人家跑去楼道哭就是不想让众人知道，你倒好，直接放上网还上了头条，己所不欲勿施于人啊", "parent": "4608870502371101", "t": "1614325800"}, {"uid": "6591176665", "mid": "4608909363122498", "user_name": "陆可水", "text": "他想表达什么呢？别人同意了吗？", "parent": "4608878949702024", "t": "1614333060"}, {"uid": "1905193195", "mid": "4608878194464654", "user_name": "xcsong_22", "text": "就像我拿到我妈癌症诊断的报告后一样，给我姨妈打电话泣不成声。但是还好治好了，五年了，没有复发。感谢北大第一医院泌尿外科的医生。", "parent": "4608870502371101", "t": "1614325620"}, {"uid": "5612112229", "mid": "4611899617185659", "user_name": "基金小咖", "text": "这不是广告", "parent": "4608878194464654", "t": "1615045980"}, {"uid": "5367926793", "mid": "4608870792301017", "user_name": "有一种花叫鹤望兰", "text": "人世间只有死别我们真的无能为力", "parent": "4608870502371101", "t": "1614323880"}, {"uid": "6865990736", "mid": "4608873526725736", "user_name": "__Zmay", "text": "就这样随便拍人家啊？还放网上，这不好吧", "parent": "4608870502371101", "t": "1614324540"}, {"uid": "3865860818", "mid": "4608872986450400", "user_name": "章惜521", "text": "拍这个发出来经人家同意了吗", "parent": "4608870502371101", "t": "1614324360"}, {"uid": "6554564127", "mid": "4608898042431272", "user_name": "遇事不决胺碘酮", "text": "就不该录", "parent": "4608872986450400", "t": "1614330360"}, {"uid": "1595876754", "mid": "4608880178627395", "user_name": "大王不巡山89", "text": "回复@Prado307:认识的人一看就知道了，认识的人也并不是所有人关系都好的。本来就是私下的情绪，未必想让人知道", "parent": "4608873690042610", "t": "1614326100"}, {"uid": "6816163940", "mid": "4608873690042610", "user_name": "Prado307", "text": "背影倒还好", "parent": "4608872986450400", "t": "1614324540"}, {"uid": "2425899100", "mid": "4608871106091199", "user_name": "面姐紫微斗数", "text": "成年人的世界里没有容易二字 加油！", "parent": "4608870502371101", "t": "1614323940"}, {"uid": "2122455550", "mid": "4608872218101404", "user_name": "鹿羊", "text": "生命太短暂，没时间遗憾，一分钟都不要留给那些让你不快的人或事。若不是终点，请微笑一直向前。", "parent": "4608870502371101", "t": "1614324180"}, {"uid": "7395485516", "mid": "4608871802342637", "user_name": "iSupremenews", "text": "这种何必拍上网", "parent": "4608870502371101", "t": "1614324120"}, {"uid": "5724189748", "mid": "4608884469403875", "user_name": "呲牙咧嘴的小猪猪", "text": "哪里打拳了，我咋没看到", "parent": "4608870502371101", "t": "1614327120"}, {"uid": "6358913552", "mid": "4608958213652920", "user_name": "连子的梦露", "text": "回复@襄樊的豆腐面:看见了……实属脑子有问题", "parent": "4608907987128274", "t": "1614344700"}, {"uid": "6358913552", "mid": "4608936693207582", "user_name": "连子的梦露", "text": "回复@襄樊的豆腐面:我康康", "parent": "4608907987128274", "t": "1614339600"}, {"uid": "7494637369", "mid": "4608907987128274", "user_name": "襄樊的豆腐面", "text": "回复@叶子的铭:评论最下面那个", "parent": "4608884469403875", "t": "1614332700"}, {"uid": "2493024515", "mid": "4608894392861747", "user_name": "啥事比亚", "text": "我也没看到。。有些人是不是复读机到哪都发一样的字挑拨？", "parent": "4608884469403875", "t": "1614329460"}, {"uid": "6358913552", "mid": "4608892749219598", "user_name": "连子的梦露", "text": "我也想知道", "parent": "4608884469403875", "t": "1614329100"}, {"uid": "3549418297", "mid": "4608886252246420", "user_name": "你从狮山来", "text": "是啊，我还等着去喷呢，结果没找着", "parent": "4608884469403875", "t": "1614327540"}, {"uid": "5488943001", "mid": "4608872025169078", "user_name": "淡紫紫野花", "text": "这个也打拳？是不是精神病 啊", "parent": "4608870502371101", "t": "1614324180"}, {"uid": "1970393643", "mid": "4608889989633655", "user_name": "莫维他", "text": "人家已经很崩溃了，拍摄者把人家最伤心的一幕拍下来，还传播到网上，经过人家同意了吗？现在自媒体时代，人人都可以随便拍别人，好的坏的都拿到网上肆意传播，这样真的好吗？人们还有一点隐私权吗？", "parent": "4608870502371101", "t": "1614328440"}, {"uid": "5986929427", "mid": "4608870717064599", "user_name": "乔乔植物园", "text": "自己去医院查出来生大病是人间很残忍的事情", "parent": "4608870502371101", "t": "1614323820"}, {"uid": "2848436913", "mid": "4608881572186971", "user_name": "嗒嗒啦嗒啦啦啦嗒嗒", "text": "人到中年的无奈", "parent": "4608870717064599", "t": "1614326400"}, {"uid": "1805750902", "mid": "4608872201585546", "user_name": "viewheng", "text": "是的，上有老下有小，自己面临重病后面又没法继续挣钱，简直不要太难", "parent": "4608870717064599", "t": "1614324180"}, {"uid": "2667831582", "mid": "4608872863498896", "user_name": "yoyokulala", "text": "爷爷年初因为肺癌走了，我觉得我一辈子都过不去这坎，意难平，白天工作，只能晚上偷偷的哭…无奈人世间为什么要有生离死别", "parent": "4608870502371101", "t": "1614324360"}, {"uid": "2667831582", "mid": "4608899464563287", "user_name": "yoyokulala", "text": "回复@微微待晞:抱抱你", "parent": "4608884418809933", "t": "1614330720"}, {"uid": "1255724800", "mid": "4608884418809933", "user_name": "微微待晞", "text": "回复@JSJSLL:抱抱，我父亲八年前心脏病突然离世，没留下只言片语没人守在身旁，这也是我一辈子过不去的坎儿。到现在想起来都止不住泪流满面。体会过的人，才知道这是什么痛。", "parent": "4608877762973697", "t": "1614327120"}, {"uid": "2667831582", "mid": "4608879461406136", "user_name": "yoyokulala", "text": "回复@otoole:", "parent": "4608875631741782", "t": "1614325920"}, {"uid": "2667831582", "mid": "4608879420509094", "user_name": "yoyokulala", "text": "回复@JSJSLL:", "parent": "4608877762973697", "t": "1614325920"}, {"uid": "2667831582", "mid": "4608879360741248", "user_name": "yoyokulala", "text": "回复@我就普通一个人:在一起住了十多年", "parent": "4608876290249461", "t": "1614325920"}, {"uid": "1733580075", "mid": "4608877762973697", "user_name": "JSJSLL", "text": "真是意难平，妈妈患病去世，至今不能接受", "parent": "4608872863498896", "t": "1614325500"}, {"uid": "7409406310", "mid": "4608876290249461", "user_name": "我就普通一个人", "text": "回复@yoyokulala:您和爷爷的感情很深，我倒很羡慕您的爷爷。", "parent": "4608875589797639", "t": "1614325140"}, {"uid": "2137318143", "mid": "4608875631741782", "user_name": "otoole", "text": "人能顺利的活到老年其实已经蛮幸运了，我身边还有人刚结婚就因病去世，有人孩子还未长大自己就得绝症", "parent": "4608872863498896", "t": "1614325020"}, {"uid": "2667831582", "mid": "4608875589797639", "user_name": "yoyokulala", "text": "回复@我就普通一个人:谢谢亲安慰，可是子欲孝而亲不待，想到这里就揪心的难过", "parent": "4608874155610880", "t": "1614325020"}, {"uid": "7409406310", "mid": "4608874155610880", "user_name": "我就普通一个人", "text": "🙏，他有你们爱已经很好了。", "parent": "4608872863498896", "t": "1614324660"}, {"uid": "3333785714", "mid": "4608880417444480", "user_name": "李狗蛋儿der", "text": "为什么偷拍别人，人家在楼道里就是不想让别人看见，你偷拍拿来博眼球真是厚颜无耻", "parent": "4608870502371101", "t": "1614326160"}, {"uid": "1415283560", "mid": "4608905718795849", "user_name": "easeminder", "text": "这也是隐私，不经人同意随便摄像公布还提供给媒体，，，现在人都如此不尊重别人吗", "parent": "4608870502371101", "t": "1614332160"}, {"uid": "1805750902", "mid": "4608870988648279", "user_name": "viewheng", "text": "其实更难的是人到中年上有老下有小，自己又重病不能挣钱了，这种才是最难的", "parent": "4608870502371101", "t": "1614323880"}, {"uid": "3865860818", "mid": "4608873245966692", "user_name": "章惜521", "text": "不过是医院每天都在上演的人间悲剧", "parent": "4608870502371101", "t": "1614324420"}, {"uid": "1691706993", "mid": "4608949954809847", "user_name": "南昌市的罗汉是杀不完的", "text": "陪我爹在湘雅住院一个月，看多了人间苦楚，唉。", "parent": "4608870502371101", "t": "1614342720"}, {"uid": "1694581075", "mid": "4608907617764953", "user_name": "喜东东teaki", "text": "我也在医院的楼道里放声哭过，还有路过的好心人给我纸巾，唉……", "parent": "4608870502371101", "t": "1614332640"}, {"uid": "2316288447", "mid": "4608875556767479", "user_name": "--亮仔--", "text": "在医院上班，感觉这样的事很正常。生命很脆弱，请大家珍爱生命，少熬夜吧。", "parent": "4608870502371101", "t": "1614325020"}, {"uid": "2408696441", "mid": "4608913217427363", "user_name": "水丽净水器", "text": "成年人的压力360度无处不在，生活真心不易。", "parent": "4608870502371101", "t": "1614333960"}, {"uid": "2096647581", "mid": "4608895819451901", "user_name": "-爱健身的小彭", "text": "只有经历过的人，才能知道其中的痛苦。", "parent": "4608870502371101", "t": "1614329820"}, {"uid": "1582679007", "mid": "4608944225393672", "user_name": "黑煤球···", "text": "甲状腺癌刚手术完出院…同病房的两个阿姨都是直肠癌化疗…有一个阿姨五年了复发了…癌细胞扩散了…不知道怎么安慰…住了几天医院看到了也听到了不少各种病情…只能说…好好爱自己…每年体检很必要…发现了尽早治疗…希望每个人都健康！", "parent": "4608870502371101", "t": "1614341340"}, {"uid": "5466017685", "mid": "4608892564932673", "user_name": "云游天下方知处", "text": "兄弟，挺住啊！", "parent": "4608870502371101", "t": "1614329040"}, {"uid": "1407207421", "mid": "4608877309985036", "user_name": "豆爸李晓康", "text": "医院每天都上演这样的人间悲剧⋯", "parent": "4608870502371101", "t": "1614325440"}, {"uid": "3913558111", "mid": "4608877024515746", "user_name": "生活你有什么了不起", "text": "三十二岁，公公心脏病加高血压，经常住院，婆婆肺癌。孩子上小学，平时上班，医院，还得照顾孩子！难！", "parent": "4608870502371101", "t": "1614325320"}, {"uid": "5087795431", "mid": "4608874160853272", "user_name": "福尔摩斯打个嗝", "text": "祝健康", "parent": "4608870502371101", "t": "1614324660"}, {"uid": "2292792591", "mid": "4609028137157920", "user_name": "Wonthin-", "text": "就这特么还要延迟退休，以后怕是在医院连个照顾的人都没有", "parent": "4608870502371101", "t": "1614361380"}, {"uid": "5996983275", "mid": "4608944234562529", "user_name": "莫抚琴但抚鱼", "text": "唉 都这样 我有一次也是坐在手机店放声大哭…店员觉得我太可怜都不敢赶我走", "parent": "4608870502371101", "t": "1614341340"}, {"uid": "1706062391", "mid": "4608918041399926", "user_name": "溪下沙", "text": "我都不知道多少次，夜里独自在医院的楼道偷偷哭泣。辞掉工作，放弃自由，反反复复陪护住院。瞒了父亲一个月后，还是告诉了实情。经历过一次抢救，好在最后挺过来了。目前陪父化疗第六次，正在医院病房里写下了这些字。", "parent": "4608870502371101", "t": "1614335100"}, {"uid": "6358913552", "mid": "4608957354875003", "user_name": "连子的梦露", "text": "太难了，要坚强，加油", "parent": "4608918041399926", "t": "1614344520"}, {"uid": "1845959370", "mid": "4608931466581282", "user_name": "蓝天一色-_-", "text": "不易！！！", "parent": "4608918041399926", "t": "1614338340"}, {"uid": "5