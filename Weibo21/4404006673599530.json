[{"uid": "5359972175", "mid": "4404006673599530", "user_name": "屌炸天学姐", "text": "外卖小哥路边救人，救人后结果自己的车子被偷走，小哥说：不后悔，一个人，一个车，谁更重要。", "parent": null, "t": "1565480460"}, {"uid": "6598208811", "mid": "4404008909059090", "user_name": "小桥流水93044", "text": "开车违章拍的那么清楚，为什么一个偷车的拍不到，是因为没有经济效益吗？", "parent": "4404006673599530", "t": "1565481000"}, {"uid": "6598208811", "mid": "4404258629367371", "user_name": "小桥流水93044", "text": "回复@正YY1987:估计你也快出名了，别碰到重庆开奇瑞的就好。", "parent": "4404008909059090", "t": "1565540520"}, {"uid": "1734488974", "mid": "4404239978100763", "user_name": "云中耀阳1987", "text": "你以为所有的违章都会被拍？我经常实线变道超速也没被拍啊", "parent": "4404008909059090", "t": "1565536080"}, {"uid": "1862406585", "mid": "4404011967288969", "user_name": "保定人在保定", "text": "怎么没有，人家拿着分红呢", "parent": "4404008909059090", "t": "1565481720"}, {"uid": "3871019292", "mid": "4404007756192983", "user_name": "诗筱韵丶", "text": "偷车的人太损了", "parent": "4404006673599530", "t": "1565480700"}, {"uid": "2269574347", "mid": "4404017969315244", "user_name": "Bernice森森", "text": "最缺德的是还有人偷外卖，外卖和快递车子箱子上的锁其实没那么结实....", "parent": "4404007756192983", "t": "1565483160"}, {"uid": "7278948539", "mid": "4404080602298936", "user_name": "曲木木曲198412", "text": "你是不知道，偷餐的都有", "parent": "4404007756192983", "t": "1565498100"}, {"uid": "1261285115", "mid": "4404007264763799", "user_name": "当下手帐", "text": "善良的人总是收到伤害，偷车的人让利奇马带走吧", "parent": "4404006673599530", "t": "1565480580"}, {"uid": "1955975121", "mid": "4404013120684166", "user_name": "热心老表", "text": "我来看你啦", "parent": "4404006673599530", "t": "1565481960"}, {"uid": "6615351229", "mid": "4404114891161353", "user_name": "你的鼻屎有点咸", "text": "为什么是外卖小哥救了呢？旁边那么多人？又为什么偏偏是饿了么外卖小哥呢？美团不行吗？", "parent": "4404006673599530", "t": "1565506260"}, {"uid": "5269474956", "mid": "4404012142841516", "user_name": "i小小卫", "text": "真正善良的人会将生命看的至高无上   崇拜了", "parent": "4404006673599530", "t": "1565481780"}, {"uid": "5396206517", "mid": "4404028144051064", "user_name": "复杂的豆腐乳", "text": "人帅心又善良的小哥哥，以后会走大运的", "parent": "4404006673599530", "t": "1565485560"}, {"uid": "6498209576", "mid": "4404007944239129", "user_name": "因缘果报108", "text": "这小偷太没人性，这小哥是成大事的人，受助者应賠他", "parent": "4404006673599530", "t": "1565480760"}, {"uid": "6589337147", "mid": "4404019441591651", "user_name": "叶帅帅给您比心了", "text": "说是这么说，还不是晚上躲起来偷偷的哭", "parent": "4404006673599530", "t": "1565483520"}, {"uid": "6869305756", "mid": "4404008132940052", "user_name": "乌鱼日记123", "text": "有人在做坏事，有人在做好事，这就是世界本来的样子", "parent": "4404006673599530", "t": "1565480820"}, {"uid": "6410669431", "mid": "4404138450180750", "user_name": "最后的白露剑", "text": "趁火打劫的人真可恶", "parent": "4404006673599530", "t": "1565511840"}, {"uid": "7256533327", "mid": "4404017360336580", "user_name": "用户7256533327", "text": "转发微博", "parent": "4404006673599530", "t": "1565482980"}, {"uid": "3186209121", "mid": "4404011501333566", "user_name": "郭小明164cm", "text": "转发微博", "parent": "4404006673599530", "t": "1565481600"}]