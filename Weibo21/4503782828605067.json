[{"uid": "6543445423", "mid": "4503782828605067", "user_name": "零零后美女2", "text": "#绍兴越城区城管上演全武行##农民工##绍兴#绍兴超话5月11日早晨十点左右，绍兴市越城区风江路城南街道城管在温馨花园暴力抢夺一位农民工的家养犬只，该犬已缚牵引绳，几个城管用武力抢夺老百姓的私人财产，这种土匪作风激起民愤。#不忘初心##不忘初心 牢记使命#\n现在是文明社会，政府和执法部门都应该文明执法，用沟通和协商的态度作为工作基本方式，而不是用暴力管制自己的市民，政府不是应该保护老百姓的人身安全和私有财产吗？\n\n强烈要求对暴力执法的工作人员向公众道歉认错，改正工作方式，端正工作态度，并且对暴力执法的工作人员给与处分。同时返还农民工的私有财产。@中国新闻网 @新闻晨报 @新浪户外 @央视新闻 @央视新闻调查 @中国普法 @中国文明网 @中国政府网 @中国新闻网 @中国新闻周刊 @人民日报 @人民日报海外版-海外网 @人民网 @人民微博 @新华网 @新华视点 @联合国 @世界卫生组织 @浙江发布 @绍兴发布 O网页链接", "parent": null, "t": "1589268960"}, {"uid": "2987160615", "mid": "4503938500048732", "user_name": "月野猫ss", "text": "最后有结果吗 狗还活着吗", "parent": "4503782828605067", "t": "1589306040"}, {"uid": "2987160615", "mid": "4503940576175517", "user_name": "月野猫ss", "text": "真的是被气到不行", "parent": "4503782828605067", "t": "1589306520"}, {"uid": "1284039780", "mid": "4503897500860749", "user_name": "tu-_-tu", "text": "看我给你的私信", "parent": "4503782828605067", "t": "1589296260"}, {"uid": "2612128665", "mid": "4504103416051028", "user_name": "西野ci-", "text": "为啥上不了热搜", "parent": "4503782828605067", "t": "1589345340"}, {"uid": "1934047295", "mid": "4503918703080922", "user_name": "kevin观音兵", "text": "我真的不忍心看到这些新闻 对了博主 你可以联系上海人大的余思彦代表 他是唯一一个为宠物发声的人大代表，希望你可以联系他 为我们发声", "parent": "4503782828605067", "t": "1589301360"}, {"uid": "6540850995", "mid": "4503924725935209", "user_name": "严格审核领养人", "text": "强盗行为 有点权利 没了人性", "parent": "4503782828605067", "t": "1589302740"}, {"uid": "6461530905", "mid": "4504017709209237", "user_name": "rvbhdxhnvd", "text": "", "parent": "4503782828605067", "t": "1589324940"}, {"uid": "7347360703", "mid": "4503949975890807", "user_name": "风卷残云987", "text": "快转微博", "parent": "4503782828605067", "t": "1589308800"}, {"uid": "7316028587", "mid": "4503848360797407", "user_name": "·缪缪", "text": "转发微博", "parent": "4503782828605067", "t": "1589284560"}, {"uid": "7157359571", "mid": "4503843038453418", "user_name": "·小手冰冰凉", "text": "转发微博", "parent": "4503782828605067", "t": "1589283