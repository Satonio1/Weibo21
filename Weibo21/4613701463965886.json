[{"uid": "2890310212", "mid": "4613701463965886", "user_name": "重庆全城搜罗", "text": "#货车司机深夜开车自扇耳光提神# 【货车司机回应深夜扇耳光提神：很常见，只是家常便饭】近日，湖北咸宁，一段货车司机深夜开车扇自己耳光提神的视频走红网络。当事人卢先生表示：这对货车司机来说，只是家常便饭，是很常见的一幕。很多网友在评论区感叹赚钱不易，生活不易，纷纷表示“心疼卢先生”。但小编也要提醒：安全驾车，切勿疲劳驾驶哦。（沸点视频）L沸点视频的微博视频 2重庆#重庆分享#", "parent": null, "t": "1615475580"}, {"uid": "2949047254", "mid": "4613701640130770", "user_name": "重庆生活播报", "text": "#货车司机深夜开车自扇耳光提神# 【货车司机回应深夜扇耳光提神：很常见，只是家常便饭】", "parent": "4613701463965886", "t": "1615475640"}, {"uid": "5527745529", "mid": "4614437333371028", "user_name": "晓悦山绪", "text": "辛苦", "parent": "4613701463965886", "t": "1615651020"}, {"uid": "6042545009", "mid": "4614437904061585", "user_name": "未经你允许Feidudu", "text": "我有一天坐在公交车上，看到一个外卖小哥在推着电瓶车走，我猜应该是没电了，觉得这个小哥好惨，他可能会被骂，被差评，被扣钱，但没人知道他是怎么把外卖送到的，所以让我们能善待努力工作的每一个人，不要轻易给一个人差评！", "parent": "4613701463965886", "t": "1615651140"}, {"uid": "2284096237", "mid": "4613838093161299", "user_name": "雨滴的背面", "text": "真出事儿了怎么办", "parent": "4613701463965886", "t": "1615508160"}, {"uid": "6884282294", "mid": "4613818707608490", "user_name": "褚芳馨Hogan", "text": "我们是拍大腿", "parent": "4613701463965886", "t": "1615503540"}, {"uid": "2949047254", "mid": "4613701640130840", "user_name": "重庆生活播报", "text": "#货车司机深夜开车自扇耳光提神# 【货车司机回应深夜扇耳光提神：很常见，只是家常便饭】", "parent": "4613701463965886", "t": "1615475640"}, {"uid": "5527745529", "mid": "4614437333371098", "user_name": "晓悦山绪", "text": "辛苦", "parent": "4613701463965886", "t": "1615651020"}, {"uid": "6665177245", "mid": "4613872822261172", "user_name": "吾佳隆Daniel", "text": "可以准备点水，困得时候洗洗会好点", "parent": "4613701463965886", "t": "1615516440"}, {"uid": "6860302461", "mid": "4613850479985941", "user_name": "宝美偲_周全", "text": "我晚上加班脑袋不灵光的时候也会这样，工作做不完又要赶工，没办法啦", "parent": "4613701463965886", "t": "1615511100"}, {"uid": "2946581154", "mid": "4613701892578560", "user_name": "吃喝玩乐High重庆", "text": "#货车司机深夜开车自扇耳光提神# 【货车司机回应深夜扇耳光提神：很常见，只是家常便饭】", "parent": "4613701463965886", "t": "1615475700"}, {"uid": "2159521054", "mid": "46137017712