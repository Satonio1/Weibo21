[{"uid": "5898199949", "mid": "4393544388991524", "user_name": "漠然死去", "text": "西安到昆明的165次车，桥被冲毁，中部车厢掉江里，刚发生的。好好的过每一天吧！不是每个人都有明天的！无常啊！为不实消息。 2金华·永康 O网页链接", "parent": null, "t": "1562986020"}, {"uid": "6536948375", "mid": "4393725759013749", "user_name": "瓦伲大叔de茶屋", "text": "这是2010年发生在宝成铁路的石亭江大桥塌桥事故视频，事发时人员全部及时转移后的10多分钟内，两节车厢坠入江中，无死亡，仅三人受轻伤。事后参与救人的铁路职工47人全部受到表彰和奖励。", "parent": "4393544388991524", "t": "1563029280"}, {"uid": "2250572045", "mid": "4393671396609993", "user_name": "J十一_", "text": "以前说所有的交通里火车最安全，现在火车也不安全了", "parent": "4393544388991524", "t": "1563016320"}, {"uid": "3731249372", "mid": "4393799381911131", "user_name": "起个name真费神", "text": "这事我记得是10年八月十几号，好像是西安到昆明，因为我当时买了这列车的车票去成都转九寨沟玩，因为当时九寨沟泥石流我就退票了，去了其他地方玩，第三天看到新闻才知道这列火车发生事故，但是没人遇难", "parent": "4393544388991524", "t": "1563046860"}, {"uid": "2309040167", "mid": "4393677264855315", "user_name": "小猪佩辣奇", "text": "真的假的呀？？", "parent": "4393544388991524", "t": "1563017700"}, {"uid": "6016008517", "mid": "4393669820149340", "user_name": "熊和鱼掌可兼得", "text": "最近真是多灾多难", "parent": "4393544388991524", "t": "1563015960"}, {"uid": "5516216058", "mid": "4393969691236141", "user_name": "後来lail", "text": "又在传谣", "parent": "4393544388991524", "t": "1563087420"}, {"uid": "1856939533", "mid": "4393862489530284", "user_name": "李机顶盒", "text": "转发微博", "parent": "4393544388991524", "t": "1563061860"}, {"uid": "1006177072", "mid": "4393704683209367", "user_name": "梦姐姐1998", "text": "转发微博", "parent": "4393544388991524", "t": "1563024240"}, {"uid": "2309040167", "mid": "4393677265293741", "user_name": "小猪佩辣奇", "text": "真的假的呀？？", "parent": "4393544388991524", "t": "1563017700"}]