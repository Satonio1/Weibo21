[{"uid": "5680023541", "mid": "4362096902346173", "user_name": "红领巾一万飞", "text": "4月17日上午，沈阳市棋盘山开发区英达街道与沈北辉山街道交界处突发火情。事故发生后，沈阳市森林公安局及时进行侦查，第一时间将野外违规用火者抓获。经查违规用火者是富某，男，46岁，现居住英达街道东沟村。17日上午，富某在耕地内烧秸秆，由于当时风力较大，引燃周围荒草。目前，富某因涉嫌在防火戒严期内违法野外用火，已被沈阳市森林公安局行政拘留。 2沈阳·棋盘山风景区 O网页链接", "parent": null, "t": "1555488360"}, {"uid": "6689100698", "mid": "4362100740828500", "user_name": "氵番广宇", "text": "谣言，请删除。", "parent": "4362096902346173", "t": "1555489320"}, {"uid": "5680023541", "mid": "4362395570161592", "user_name": "红领巾一万飞", "text": "回复@诺手有福:那么你微博搜搜沈阳棋盘山吧", "parent": "4362100740828500", "t": "1555559580"}, {"uid": "6788915204", "mid": "4362375064717794", "user_name": "虎牙刘大大", "text": "回复@红领巾一万飞:谣传", "parent": "4362179463228474", "t": "1555554720"}, {"uid": "5680023541", "mid": "4362179463228474", "user_name": "红领巾一万飞", "text": "回复@潘广宇想变强:他说他的吧，我说我的，你明天看微信新闻就知道了", "parent": "4362100740828500", "t": "1555508040"}, {"uid": "6689100698", "mid": "4362167791082461", "user_name": "氵番广宇", "text": "回复@红领巾一万飞:你自己那微博上边官方都写了谣言了，你还在这查啥呢？", "parent": "4362166259471569", "t": "1555505280"}, {"uid": "5680023541", "mid": "4362166259471569", "user_name": "红领巾一万飞", "text": "回复@潘广宇想变强:你查一下微博吧你", "parent": "4362100740828500", "t": "1555504920"}, {"uid": "6689100698", "mid": "4362157547830967", "user_name": "氵番广宇", "text": "回复@红领巾一万飞:唉", "parent": "4362150430232508", "t": "1555502820"}, {"uid": "5680023541", "mid": "4362150430232508", "user_name": "红领巾一万飞", "text": "你凭什么说是谣言呢？", "parent": "4362100740828500", "t": "1555501140"}, {"uid": "1593263770", "mid": "4362097733569551", "user_name": "勍了个晴", "text": "切斤卖了也赔不起这山啊！千万别有人员伤亡了，太坑人了简直", "parent": "4362096902346173", "t": "1555488600"}, {"uid": "5680023541", "mid": "4362104062024050", "user_name": "红领巾一万飞", "text": "周内的住户也被烧了，判刑得有年头了", "parent": "4362097733569551", "t": "1555490100"}, {"uid": "5512674003", "mid": "4362106767173155", "user_name": "假如从噺來過", "text": "说是谣言的人  你是他什么人啊", "parent": "4362096902346173", "t": "1555490760"}, {"uid": "6689100698", "mid": "4362157619369567", "user_name": "氵番广宇", "text": "？？？？", "parent": "4362106767173155", "t": "1555502880"}, {"uid": "3110829053", "mid": "4362152967806207", "user_name": "Treasure丶宇", "text": "你还不删谣言？等着被抓？", "parent": "4362096902346173", "t": "1555501740"}, {"uid": "6689100698", "mid": "4362157514806898", "user_name": "氵番广宇", "text": "@沈阳网警小胖 @沈阳网警巡查执法 @沈阳网安", "parent": "4362096902346173", "t": "1555502820"}]