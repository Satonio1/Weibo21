[{"uid": "1259813510", "mid": "4480578185130010", "user_name": "顾言右", "text": "一个小故事，应该是编撰的，某人叫吴好运，今年62岁，熬过了三年自然灾害，在唐山大地震废墟中生存，03年非典爆发的时候，在一线抗疫，后去北川祭祖又遭遇汶川大地震，这次在新冠肆虐下他虽感染却还是顽强的活了下来。可这次来到福建却被倒塌的欣佳宾馆压死了。他一次次躲过了天灾，但唯一一次死在了人祸。", "parent": null, "t": "1583736540"}, {"uid": "6862889557", "mid": "4480578613292108", "user_name": "从前有座山山上有条河", "text": "严肃追责！一查到底", "parent": "4480578185130010", "t": "1583736600"}, {"uid": "5228977593", "mid": "4480578751370803", "user_name": "我允许你喜欢我一次", "text": "nan", "parent": "4480578185130010", "t": "1583736660"}, {"uid": "6862889557", "mid": "4480961389638577", "user_name": "从前有座山山上有条河", "text": "天啊，原来这个消息是真的，我还以为是编撰的呢？", "parent": "4480578185130010", "t": "1583827860"}, {"uid": "6440332379", "mid": "4480723559487792", "user_name": "17工作室", "text": "造谣", "parent": "4480578185130010", "t": "1583771160"}, {"uid": "6404977666", "mid": "4480655604000539", "user_name": "sgcb15", "text": "23号出院能离开武汉吗？吴好运谐音是不好运的意思，谁会起这个名字呢，编故事的人也太无聊了。", "parent": "4480578185130010", "t": "1583754960"}, {"uid": "5373720831", "mid": "4480668840831711", "user_name": "石狮八卦", "text": "你一个大V号居然敢造谣，该当何罪。", "parent": "4480578185130010", "t": "1583758140"}, {"uid": "1079782401", "mid": "4480631512127544", "user_name": "傆神", "text": "一查到底，把造谣者揪出来。", "parent": "4480578185130010", "t": "1583749260"}, {"uid": "5268472722", "mid": "4480635995204224", "user_name": "浓茶抱咖啡", "text": "", "parent": "4480578185130010", "t": "1583750280"}, {"uid": "5228977593", "mid": "4480578751370966", "user_name": "我允许你喜欢我一次", "text": "", "parent": "4480578185130010", "t": "1583736660"}, {"uid": "6862889557", "mid": "4480578613504591", "user_name": "从前有座山山上有条河", "text": "严肃追责！一查到底", "parent": "4480578185130010", "t": "1583736600"}, {"uid": "6862889557", "mid": "4480578562734397", "user_name": "从前有座山山上有条河", "text": "快转微博", "parent": "4480578185130010", "t": "1583736600"}]