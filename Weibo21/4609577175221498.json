[{"uid": "1720962692", "mid": "4609577175221498", "user_name": "楚天都市报极目新闻", "text": "【#女子墓园内用袋子装走他人贡品# ，目击者：当时还有人正在祭拜】2月28日，贵州黔西南。有网友发视频，兴义市某墓葬园，一女子手提大塑料袋，装走他人贡品。目击者称，该女子是在墓地挨个收贡品，当时墓园还有人在祭拜，看着很不舒服。（漩涡视频） L漩涡视频的微博视频", "parent": null, "t": "1614492300"}, {"uid": "2803771310", "mid": "4609578941022317", "user_name": "你咋不上天讷", "text": "就怕是拿了再拿出去卖的", "parent": "4609577175221498", "t": "1614492720"}, {"uid": "6851782544", "mid": "4609577699772467", "user_name": "用户名13652346", "text": "都混到跟死人要饭的境界了，就随她去吧", "parent": "4609577175221498", "t": "1614492420"}, {"uid": "6815781703", "mid": "4609577477213957", "user_name": "一只没有感情的小喵喵", "text": "这是手有多欠，等你家坟头上祭品被拿了你自己感受一下是什么感觉，有点良知的", "parent": "4609577175221498", "t": "1614492360"}, {"uid": "3550389397", "mid": "4609578107142316", "user_name": "唐帅mtvmtv", "text": "供完后就让人家拿走吧，烂也是烂了", "parent": "4609577175221498", "t": "1614492480"}, {"uid": "1864911490", "mid": "4609582515622857", "user_name": "叽叽喳的李大鸟", "text": "正常的很啊，难不成摆放到下一次祭拜？都是有人收的，只不过这次收的早了，吃相不好", "parent": "4609577175221498", "t": "1614493560"}, {"uid": "5181313276", "mid": "4609578363521562", "user_name": "元固心", "text": "就不怕晚上人家去找她？这点便宜都占，贼！", "parent": "4609577175221498", "t": "1614492540"}, {"uid": "1869110852", "mid": "4609613134042389", "user_name": "想要冬眠的西瓜", "text": "与本文无关，但就是突然想到有一次去扫墓，远远的一条刚生过崽的流浪狗叼着一只贡品馒头小心的埋在土里，当时还在下小雨，她一脸戒备生怕我们抢她的馒头，超可怜。", "parent": "4609577175221498", "t": "1614500820"}, {"uid": "3411764574", "mid": "4609579206579463", "user_name": "XQWONG", "text": "会有报应的，敢这样做就要接受因果。承担不起自己倒霉。", "parent": "4609577175221498", "t": "1614492780"}, {"uid": "6439480730", "mid": "4609592838066805", "user_name": "过往的晴天GG", "text": "快转微博", "parent": "4609577175221498", "t": "1614496020"}]