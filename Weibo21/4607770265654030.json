[{"uid": "2952435102", "mid": "4607770265654030", "user_name": "北京全搜罗", "text": "#重走货拉拉遇害女生搬家路线#【重走货拉拉遇害女生搬家路线：比正常路线慢至少6分钟，警方表示现场看不到刹车痕迹】#女孩搬家跳窗身亡现场无刹车痕迹# 近日，湖南长沙23岁女孩用货拉拉搬家跳窗身亡一事，引发关注。2月22日晚，家属和记者一起重走货拉拉偏航路线，发现偏航路线比正常路线慢至少6分钟。到达出事地点时，还遇到警方用货拉拉货车连夜勘查。此外，家属还透露，警方告知现场没有明显紧急刹车痕迹，这成为一个新的疑点。 （封面新闻）L重走跳车女孩事发路线：多路段无路灯 警方在... 2北京 #北京热点关注#", "parent": null, "t": "1614061500"}, {"uid": "2949878914", "mid": "4607771364824040", "user_name": "厦门全搜罗", "text": "#重走货拉拉遇害女生搬家路线#：比正常路线慢至少6分钟，警方表示现场看不到刹车痕迹！", "parent": "4607770265654030", "t": "1614061740"}, {"uid": "2143399741", "mid": "4607771243447265", "user_name": "北京城事话题", "text": "#重走货拉拉遇害女生搬家路线#：比正常路线慢至少6分钟，警方表示现场看不到刹车痕迹！", "parent": "4607770265654030", "t": "1614061680"}, {"uid": "2949889350", "mid": "4607771116571685", "user_name": "吃喝玩乐High北京", "text": "#重走货拉拉遇害女生搬家路线#：比正常路线慢至少6分钟，警方表示现场看不到刹车痕迹！", "parent": "4607770265654030", "t": "1614061680"}, {"uid": "2834458534", "mid": "4607770940410139", "user_name": "我爱美丽北京", "text": "#重走货拉拉遇害女生搬家路线#：比正常路线慢至少6分钟，警方表示现场看不到刹车痕迹！", "parent": "4607770265654030", "t": "1614061620"}, {"uid": "7357227178", "mid": "4608803537158717", "user_name": "horrible乌鸦嘴球探传媒", "text": "刹车痕迹如果不是大货车急刹、小货车和小客车急刹很难看到刹车痕迹。。", "parent": "4607770265654030", "t": "1614307800"}, {"uid": "6391805026", "mid": "4607793900558217", "user_name": "轻薄昔瑛", "text": "希望尽早真相大白。还死者一个公道……", "parent": "4607770265654030", "t": "1614067080"}, {"uid": "6855754237", "mid": "4607779216557152", "user_name": "中看犹萱彤", "text": "不刹车明显是做贼心虚！！！！！！", "parent": "4607770265654030", "t": "1614063600"}, {"uid": "6148918352", "mid": "4607860175279669", "user_name": "Zeus5204的世界", "text": "咋个明显法？", "parent": "4607779216557152", "t": "1614082920"}, {"uid": "6037662576", "mid": "4607775005481229", "user_name": "明温瑜乁", "text": "真想早点知道事情真相", "parent": "4607770265654030", "t": "1614062580"}, {"uid": "1039463644", "mid": "4607773092348587", "user_name": "2018哦豁", "text": "记者是两条路都走了，还是听导航报的？？？", "parent": "4607770265654030", "t": "1614062160"}, {"uid"