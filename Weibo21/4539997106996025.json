[{"uid": "3275124562", "mid": "4539997106996025", "user_name": "密探先生", "text": "#四川乐山通报疑似化工厂泄漏#紧急撤离！！大家注意安全", "parent": null, "t": "1597903080"}, {"uid": "5533802127", "mid": "4539998000393772", "user_name": "欲意者k", "text": "说话不嫌腰疼。如果真是有毒气体，医院早躺满人了，多少记者去当地采访，只是闻到了有臭鸡蛋一样的味道。有就是有，没有就是没有，不要把所有事情都夸大，确实有气体泄露，但绝不是有毒气体", "parent": "4539997106996025", "t": "1597903320"}, {"uid": "1796672005", "mid": "4540030690796806", "user_name": "Gloaming1919", "text": "怎么就不是有毒气体了？那你的意思是泄露了不危险，要爆炸死人了才好吗？", "parent": "4539998000393772", "t": "1597911120"}, {"uid": "3936658436", "mid": "4540017106486563", "user_name": "看不见也找不到", "text": "你怕不知道慢性的，和后遗症的，到时候死无对证找谁去，跑总没错的", "parent": "4539998000393772", "t": "1597907880"}, {"uid": "5641814691", "mid": "4540011704227820", "user_name": "我想有条狗子", "text": "臭鸡蛋味的气体是哪几种 毒性到底有没有 你查查就知道了", "parent": "4539998000393772", "t": "1597906560"}, {"uid": "5557758041", "mid": "4539999531827307", "user_name": "林空sky", "text": "浓度不高而已。毕竟空旷的地带", "parent": "4539998000393772", "t": "1597903680"}, {"uid": "3625708877", "mid": "4539998710271799", "user_name": "果然还是无辣不欢", "text": "化工厂现在还产氯化钠呢？海盐现在缺口这么大吗", "parent": "4539997106996025", "t": "1597903500"}, {"uid": "5466650781", "mid": "4540009199440978", "user_name": "嵌着星星的二筒_", "text": "氯化钠是基础工业原料 工业盐你也吃不了啊", "parent": "4539998710271799", "t": "1597905960"}, {"uid": "3625708877", "mid": "4540013934816232", "user_name": "果然还是无辣不欢", "text": "回复@嵌着星星的二筒_:你典型的看不懂别人说啥，还想跟别人辩论型选手", "parent": "4540009199440978", "t": "1597907100"}, {"uid": "3251836415", "mid": "4539997699450178", "user_name": "柠栀栀柠栀", "text": "打码 打了个寂寞 造谣就完了", "parent": "4539997106996025", "t": "1597903260"}, {"uid": "5602270464", "mid": "4540005512905541", "user_name": "狗老师今天笑了吗", "text": "氯化钠厂……但凡有点知识乱说也能说个可信点的……", "parent": "4539997106996025", "t": "1597905120"}, {"uid": "5533802127", "mid": "4539998533061315", "user_name": "欲意者k", "text": "你这种截图被外网拿来就是一个抹黑帖，在没有任何证据的情况下，造谣说是有毒气体，你究竟葫芦里卖的什么药，造谣可耻", "parent": "4539997106996025", "t": "1597903440"}, {"uid": "1712174577", "mid": "4539999285156528", "user_name": "我被你们蠢哭了", "text": "李文亮警告", "parent": "4539997106996025", "t": "1597903620"}, {"uid": "1790296007", "mid": "4540006343907951", "user_name": "久幕观影", "text": "造谣生事", "parent": "4539997106996025", "t": "1597905300"}, {"uid": "7415142577", "mid": "4540007865385587", "user_name": "清汤辣子鸡", "text": "氯化钠是气体吗 我怎么依稀盐里有氯化钠", "parent": "4539997106996025", "t": "1597905660"}, {"uid": "6791566328", "mid": "4540007114611315", "user_name": "刚发的什么让我看看", "text": "“氯化钠炸了……对消化系统影响特别大”，笑死，当地人平时不吃盐的吗？", "parent": "4539997106996025", "t": "1597905480"}, {"uid": "7279665430", "mid": "4539997636003021", "user_name": "伏特加Finlandia56726", "text": "？？？啥迷惑行为", "parent": "4539997106996025", "t": "1597903200"}, {"uid": "2778467307", "mid": "4539998156623072", "user_name": "沉默的熊177", "text": "神奇的码", "parent": "4539997106996025", "t": "1597903320"}]