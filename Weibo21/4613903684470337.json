[{"uid": "3205468931", "mid": "4613903684470337", "user_name": "零售电商观察", "text": "#直播平台羽绒服抽检8成不达标#近日，浙江省消保委对一批网络直播平台中销售的羽绒服进行抽检测试。结果30个批次的羽绒服中，有24批次的测试结果存在不达标的情况，占比达到80%。这批羽绒服全部交由专业的第三方检测机构进行检测。检测机构就消费者关心的纤维成分含量、甲醛、充绒量、羽绒含量、蓬松度等十几项主要指标进行检测后发现，羽绒含量不足、充绒量不做标明，或者标明不清等情况十分普遍。另外，抽检中还发现了标识标签不符合国家标准，羽绒蓬松度指数不理想等问题。L央视财经的微博视频", "parent": null, "t": "1615523820"}, {"uid": "2430875120", "mid": "4613917027076654", "user_name": "晨爱夕离岁月", "text": "希望严惩", "parent": "4613903684470337", "t": "1615527000"}, {"uid": "2035019972", "mid": "4613917021838295", "user_name": "衡山-想念_512_496", "text": "直播行业好好监管吧", "parent": "4613903684470337", "t": "1615527000"}, {"uid": "2276548257", "mid": "4613917001387997", "user_name": "萝莉萝莉-777_682", "text": "退一赔三他们就不敢了", "parent": "4613903684470337", "t": "1615526940"}, {"uid": "2485518204", "mid": "4613916573046696", "user_name": "5_june_103", "text": "对那种平台和个人卖假冒伪劣产品的罚到他。亏了裤裆", "parent": "4613903684470337", "t": "1615526880"}, {"uid": "2371616587", "mid": "4613916564657891", "user_name": "那南2", "text": "牌子应该说出来", "parent": "4613903684470337", "t": "1615526880"}, {"uid": "2433077510", "mid": "4613916561248864", "user_name": "破嘎萨诱惑_796", "text": "现在羽绒服还有一两百一件的，能合格就怪了", "parent": "4613903684470337", "t": "1615526880"}, {"uid": "1764012131", "mid": "4613917001913588", "user_name": "施雁露_zQyD", "text": "直播平台买东西真的要谨慎谨慎再谨慎了", "parent": "4613903684470337", "t": "1615526940"}, {"uid": "2576902287", "mid": "4613917000861042", "user_name": "夙睿_OCad", "text": "太可怕了吧", "parent": "4613903684470337", "t": "1615526940"}, {"uid": "5759358488", "mid": "4613916992997567", "user_name": "季冷玉_fRKm", "text": "建议假一赔百", "parent": "4613903684470337", "t": "1615526940"}, {"uid": "1768639070", "mid": "4613916975700453", "user_name": "一江_酱桑", "text": "赚钱无底线", "parent": "4613903684470337", "t": "1615526940"}, {"uid": "6069930492", "mid": "4613916510393932", "user_name": "肥仔gai边_", "text": "好多直播平台带货都是没有质量保证的  跟打出去的宣传都不一样", "parent": "4613903684470337", "t": "1615526880"}]