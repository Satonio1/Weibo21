[{"uid": "5493934570", "mid": "4504071794687057", "user_name": "风云学会陈经", "text": "武汉三民小区的人全变红码，全市所有人全部核酸检测！\n\n武汉三民小区出了六个感染者，清零很久以后出的，震动全国。是一个89岁的，之前感染自己好了，没去治疗，隔了两个月又发病传染人。武汉发了狠，所有人（除六岁以下）全部强制检测！三民小区封掉，5000人都变红码动不了，又隔离14天，上班的也不行了。全市10天都测一遍，不测的人变黄码无法出行，过期检测自己出钱。要测一个小区，就提前封闭。这是搞毛了，被病毒害的太惨。大样本检测，科研意义也很大。", "parent": null, "t": "1589337840"}, {"uid": "1572407554", "mid": "4504073606433915", "user_name": "xxxxx104", "text": "不懂就问，六岁一下不测是什么原因？", "parent": "4504071794687057", "t": "1589338260"}, {"uid": "6845095374", "mid": "4504112110724375", "user_name": "听朋友说通讯社社长", "text": "应该是打了卡介苗有一定的预防作用", "parent": "4504073606433915", "t": "1589347440"}, {"uid": "6510930328", "mid": "4504109976208659", "user_name": "皮皮虾快走哇", "text": "小孩子太小了检查难度大，毕竟要一个个普查，工作量会很大，没瞎说刚刚做的检查，大一点的小孩子也可以做检查", "parent": "4504073606433915", "t": "1589346960"}, {"uid": "7217147207", "mid": "4504105022409840", "user_name": "李雨巍", "text": "回复@点点奇异果阿:祝好运", "parent": "4504086063745722", "t": "1589345760"}, {"uid": "5800463449", "mid": "4504094867449855", "user_name": "摇曳夫人17831", "text": "可以测啊。我今天刚测的，好多小孩", "parent": "4504073606433915", "t": "1589343360"}, {"uid": "2176357077", "mid": "4504092993801076", "user_name": "干420", "text": "祝福健康平安", "parent": "4504073606433915", "t": "1589342880"}, {"uid": "2380681470", "mid": "4504092052975505", "user_name": "蜜雪冰城绵阳分城", "text": "世界上没有人被儿童传染过得，测了干啥，就是确认也没事", "parent": "4504073606433915", "t": "1589342640"}, {"uid": "2112558015", "mid": "4504086840441902", "user_name": "致虚极_守静笃", "text": "六岁小孩无行为能力，只能呆在家里，家长没事，小孩也没事", "parent": "4504073606433915", "t": "1589341440"}, {"uid": "3971676708", "mid": "4504086063745722", "user_name": "点点奇异果阿", "text": "回复@长风万里送秋雁乀:我们小区说六个月以上的都要检测，我们家娃八个月", "parent": "4504074118266124", "t": "1589341260"}, {"uid": "6460921332", "mid": "4504084054807870", "user_name": "薄墨深雪", "text": "回复@xxxxx104:谢谢你", "parent": "4504080108856942", "t": "1589340780"}, {"uid": "1846623890", "mid": "4504081585013658", "user_name": "养猫的珠宝商左先生", "text": "没身份证", "parent": "4504073606433915", "t": "1589340180"}, {"uid": "1572407554", "mid": "4504080199811577", "user_name": "xxxxx104", "text": "回复@薄墨深雪:辛苦了，保护好自己。", "parent": "4504079743160970", "t": "1589339820"}, {"uid": "1572407554", "mid": "4504080108856942", "user_name": "xxxxx104", "text": "回复@SfourP:明白了，谢谢。", "parent": "4504079084316320", "t": "1589339820"}, {"uid": "6460921332", "mid": "4504079743160970", "user_name": "薄墨深雪", "text": "其实不用担心，我在三民现场，除了刚出生几个月的，小孩家长都很自觉地带孩子来检测了", "parent": "4504073606433915", "t": "1589339700"}, {"uid": "7282166192", "mid": "4504079084316320", "user_name": "SfourP", "text": "6岁以下儿童不可能单独感染，监护人的情况就代表他的情况。", "parent": "4504073606433915", "t": "1589339580"}, {"uid": "2803571371", "mid": "4504074470599719", "user_name": "人生路上一歌者-路歌", "text": "回复@xxxxx104:", "parent": "4504073606433915", "t": "1589338440"}, {"uid": "1572407554", "mid": "4504074350000500", "user_name": "xxxxx104", "text": "回复@人生路上一歌者-路歌:", "parent": "4504074470599719", "t": "1589338440"}, {"uid": "2803571371", "mid": "4504074211465026", "user_name": "人生路上一歌者-路歌", "text": "不能独自行动，测了他的监护人就等于间接了解他的情况了。监护人如果确诊，应该还是会测的。", "parent": "4504073606433915", "t": "1589338380"}, {"uid": "1939761085", "mid": "4504074118266124", "user_name": "长风万里送秋雁乀", "text": "6岁以下儿童一直在家，监护人没事儿，孩子就大概率没事儿的。再一个儿童很多诊断标准和大人不一样的。个人观点", "parent": "4504073606433915", "t": "1589338380"}, {"uid": "1095341470", "mid": "4504074109855172", "user_name": "Karrdel", "text": "估计是没几个感染的，这病毒还是怕后浪。", "parent": "4504073606433915", "t": "1589338380"}, {"uid": "2122282563", "mid": "4504072411807055", "user_name": "中年油腻猥琐难", "text": "碰上这么狡猾的病毒，就只能用最笨的办法全面检测。", "parent": "4504071794687057", "t": "1589337960"}, {"uid": "1713520817", "mid": "4504114124569817", "user_name": "v妮2014", "text": "回复@奶妈T2Q:你厉害，你别带口罩。别洗手。", "parent": "4504072411807055", "t": "1589347920"}, {"uid": "5399313695", "mid": "4504096490664119", "user_name": "Moose角鹿", "text": "回复@v妮2014:病毒只能在体内繁殖", "parent": "4504072806154303", "t": "1589343720"}, {"uid": "5567545392", "mid": "4504076639373482", "user_name": "自带体香de男人", "text": "回复@v妮2014:你的话显示出你的高智商", "parent": "4504072806154303", "t": "1589338980"}, {"uid": "6474124813", "mid": "4504076061254427", "user_name": "东8123", "text": "大智若愚！这种时候最笨的就是最聪明的。新冠专治各种不服，所有侥幸，小聪明，一律拿下！", "parent": "4504072411807055", "t": "1589338860"}, {"uid": "1713520817", "mid": "4504072806154303", "user_name": "v妮2014", "text": "家里消毒不彻底，病毒还在繁殖", "parent": "4504072411807055", "t": "1589338080"}, {"uid": "3200773224", "mid": "4504073904914042", "user_name": "绝望的小千酱", "text": "统计学病毒学可以去现场了，多好的实际案例", "parent": "4504071794687057", "t": "1589338320"}, {"uid": "1953721153", "mid": "4504114639169038", "user_name": "香菜病", "text": "啊哈，头像", "parent": "4504073904914042", "t": "1589348040"}, {"uid": "1784365354", "mid": "4504075280151314", "user_name": "商音", "text": "本来就应该在武汉解封前这样做的", "parent": "4504071794687057", "t": "1589338680"}, {"uid": "3439358104", "mid": "4504087318945336", "user_name": "执扇掩笑boy", "text": "检测了又怎样，准确率百分之六十左右，没有精力人力物力对每个人检测四五次", "parent": "4504075280151314", "t": "1589341560"}, {"uid": "2049632923", "mid": "4504086978719403", "user_name": "lucky_eagle", "text": "谁也没想到潜伏这么久", "parent": "4504075280151314", "t": "1589341440"}, {"uid": "2683505423", "mid": "4504086199076788", "user_name": "smileflower", "text": "现在检测更难了，人都流动个把月了", "parent": "4504075280151314", "t": "1589341260"}, {"uid": "2724112362", "mid": "4504077549910740", "user_name": "建晓1983", "text": "这个谈何容易啊！多大的一个工程，不到万不得已不会这么做！", "parent": "4504075280151314", "t": "1589339220"}, {"uid": "6485998618", "mid": "4504072454123774", "user_name": "长连高", "text": "这个关注，看看老美那么多感染数是不是因“检测过度”", "parent": "4504071794687057", "t": "1589337960"}, {"uid": "1897630272", "mid": "4504409835364778", "user_name": "小兔子and大兔子的小屋", "text": "回复@浅浅荷瑞:是三千万", "parent": "4504139750663402", "t": "1589418420"}, {"uid": "1617527977", "mid": "4504139750663402", "user_name": "浅浅荷瑞", "text": "就老美那德行 是检测不足 保守估计三百万人以上感染  再拖下去会有更多人感染  加之病毒反复变异 美国会有更多人死亡 这个病毒太邪恶了", "parent": "4504072454123774", "t": "1589354040"}, {"uid": "5660622725", "mid": "4504081286877143", "user_name": "地球上的少年", "text": "你是看老美涨的不够快", "parent": "4504072454123774", "t": "1589340120"}, {"uid": "6485998618", "mid": "4504078237701089", "user_name": "长连高", "text": "回复@小帆帆就是我2020:好吧", "parent": "4504072454123774", "t": "1589339340"}, {"uid": "1673618000", "mid": "4504077989553149", "user_name": "终于可以改名的小帆帆", "text": "美国才检测了几百万，中国已经检测了几千万了。美国现在不是检测过度，而是检测不足", "parent": "4504072454123774", "t": "1589339280"}, {"uid": "6485998618", "mid": "4504076241348784", "user_name": "长连高", "text": "回复@中山春天奏鸣曲:我去搜下", "parent": "4504074735058255", "t": "1589338920"}, {"uid": "1840672763", "mid": "4504074735058255", "user_name": "中山春天奏鸣曲", "text": "广州前段时间也大范围检测，比如的士司机、高三初三复课学生，阳性者极少，就是确诊患者密切接触者阳性，其他都阴性。", "parent": "4504072454123774", "t": "1589338560"}, {"uid": "7077759555", "mid": "4504075944003385", "user_name": "薛定谔的聪聪", "text": "想请教一下，有没有可能是宠物传播的呢，或者鸟类蚊虫", "parent": "4504071794687057", "t": "1589338800"}, {"uid": "5944580507", "mid": "4504621089621561", "user_name": "进击的王工", "text": "新闻说过可能是猫", "parent": "4504075944003385", "t": "1589468820"}, {"uid": "7077759555", "mid": "4504096096490130", "user_name": "薛定谔的聪聪", "text": "回复@千面丑怪:新闻说过，好像挺多动物都会携带病毒，有些也会咳嗽", "parent": "4504092908918837", "t": "1589343600"}, {"uid": "6553654702", "mid": "4504092908918837", "user_name": "千面丑怪", "text": "动物不会传播的", "parent": "4504075944003385", "t": "1589342880"}, {"uid": "3947975076", "mid": "4504079742976875", "user_name": "九十里铺", "text": "新加坡的合并检测方法可以参考一下：同一家人的核酸样本合并在一起检测，如果是阴就都是阴，如果是阳就再分开测一次。这样测会快些，因为是阳的非常少。", "parent": "4504071794687057", "t": "1589339700"}, {"uid": "6513557932", "mid": "4504137902053057", "user_name": "Liu格致", "text": "这也不是他们发明的，全球都这样！", "parent": "4504079742976875", "t": "1589353620"}, {"uid": "5856952650", "mid": "4504119878951902", "user_name": "更弗里茨恋歌", "text": "韩国也是，十人一并", "parent": "4504079742976875", "t": "1589349300"}, {"uid": "2902984781", "mid": "4504106021130091", "user_