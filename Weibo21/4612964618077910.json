[{"uid": "2949047254", "mid": "4612964618077910", "user_name": "重庆生活播报", "text": "#为什么空巢青年越来越多#【#我国空巢青年规模或升至9200万#：不愿结婚生育、沉迷加班】据统计，2018年就有超过7700万独居状态的“空巢青年”，预计2021年，“空巢青年”数据会上升到9200万。据人民智库发布的数据显示，绝大多数“空巢青年”是主动选择独居。其中，因“想有自己的隐私空间”而选择独居的占比47.5%，因“自己的生活作息和别人不同，不想受到他人影响”而独居的占比39.2%。选择“一个人生活很方便”和“没什么特别的原因，单纯地想体验独自生活”的“空巢青年”分别占比33.5%和32.2%。（新时报）2重庆 #重庆生活#", "parent": null, "t": "1615299900"}, {"uid": "2890310212", "mid": "4612964902505644", "user_name": "重庆全城搜罗", "text": "#为什么空巢青年越来越多#【我国空巢青年规模或升至9200万：不愿结婚生育、沉迷加班】", "parent": "4612964618077910", "t": "1615299960"}, {"uid": "7384330875", "mid": "4613113732925140", "user_name": "Mr_佑欣", "text": "沉迷加班？确定不是沉迷熬夜？被迫加班？", "parent": "4612964618077910", "t": "1615335480"}, {"uid": "6025913529", "mid": "4613028442014694", "user_name": "甜甜丶sweet", "text": "笑死，空巢青年，不就是我吗。不愿谈恋爱不愿和不熟悉的人出去玩只想自己一个人呆着", "parent": "4612964618077910", "t": "1615315140"}, {"uid": "6870994770", "mid": "4613028274766587", "user_name": "徐瑶瑶_裁缝", "text": "沉迷加班哪个脑子有包啊！我一点都不沉迷，天天想提前溜号", "parent": "4612964618077910", "t": "1615315080"}, {"uid": "6868070395", "mid": "4613032640774651", "user_name": "归睿范丶行李员", "text": "独处是一天 中最快乐的时刻", "parent": "4612964618077910", "t": "1615316100"}, {"uid": "6028663328", "mid": "4613035483726495", "user_name": "平恒嘉鲁莽", "text": "我觉得一个人好爽 享受自由", "parent": "4612964618077910", "t": "1615316820"}, {"uid": "6025913529", "mid": "4613028442014716", "user_name": "甜甜丶sweet", "text": "笑死，空巢青年，不就是我吗。不愿谈恋爱不愿和不熟悉的人出去玩只想自己一个人呆着", "parent": "4612964618077910", "t": "1615315140"}, {"uid": "6870994770", "mid": "4613028274766599", "user_name": "徐瑶瑶_裁缝", "text": "沉迷加班哪个脑子有包啊！我一点都不沉迷，天天想提前溜号", "parent": "4612964618077910", "t": "1615315080"}, {"uid": "2946581154", "mid": "4612965443046366", "user_name": "吃喝玩乐High重庆", "text": "#为什么空巢青年越来越多#【我国空巢青年规模或升至9200万：不愿结婚生育、沉迷加班】", "parent": "4612964618077910", "t": "1615300080"}, {"uid": "6326587992", "mid": "4612965322462761", "user_name": "烈性小刚", "text": "转发微博", "parent": "4612964618077910", "t": "1615300080"}