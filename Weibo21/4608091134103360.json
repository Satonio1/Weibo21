[{"uid": "1887344341", "mid": "4608091134103360", "user_name": "观察者网", "text": "【塞尔维亚著名主持人：我曾经也做美国梦，但中国人才是天赐的礼物】\n“对我们来说，中国人就像是天赐的礼物。”马里奇（Milomir Marić）是塞尔维亚最著名的电视节目主持人，该国总统武契奇经常到他主持的节目《开心TV》做客。\n\n据德媒2月18日报道，马里奇接受媒体采访，谈到了中国对塞尔维亚基础设施建设和疫情期间的帮助。\n\n马里奇不禁感叹道，年轻时，他曾做过“美国梦”——加入欧盟，成为西方的一部分。但现在，他相信不会有这么一天了。“中国为我们建造了欧洲不感兴趣的桥梁和道路。”", "parent": null, "t": "1614138000"}, {"uid": "6317228722", "mid": "4608091788679008", "user_name": "现实就是无法醒来的梦", "text": "做好属于自己的梦就好。但要做好实事求是的科学精神、自省自强的自力更生精神、和而不同的和平交往精神就好。", "parent": "4608091134103360", "t": "1614138120"}, {"uid": "1065197312", "mid": "4608220192833826", "user_name": "起个昵称真不容易2011", "text": "回复@大湾村首席拖拉机手:可惜了，铁托一手打造的那么强大的南斯拉夫，唉！", "parent": "4608101720529791", "t": "1614168720"}, {"uid": "1497574703", "mid": "4608141172410220", "user_name": "闲云野鹤与影同行", "text": "回复@大湾村首席拖拉机手:然而那些有着西方梦的南斯拉夫人还在骂着铁托。与中国何其相似。如果八十年代后期我们没扛住，境遇只会比南更糟糕。", "parent": "4608101720529791", "t": "1614149880"}, {"uid": "3802878924", "mid": "4608101720529791", "user_name": "大湾村首席拖拉机手", "text": "铁托的时代，南斯拉夫也有这种精神，完全不受两个超级大国挟制。然而没有了铁托，也就没有了南斯拉夫", "parent": "4608091788679008", "t": "1614140520"}, {"uid": "6839839093", "mid": "4608098872070031", "user_name": "後六合", "text": "回复@楽游星球:这要感谢教员他们这些老一辈了，他们给中华民族自尊自立自强的信心，让中华民族重新有了骨头", "parent": "4608097295010737", "t": "1614139800"}, {"uid": "7418502704", "mid": "4608097295010737", "user_name": "楽游星球", "text": "世界上二百多个国家，只有极个别才会想要自立自强。中华民族具备这种伟大能力。", "parent": "4608091788679008", "t": "1614139440"}, {"uid": "6497329749", "mid": "4608091582895681", "user_name": "滚滚蹲墙角", "text": "这个思想再进一步就是塞尔维亚曾经的路了", "parent": "4608091134103360", "t": "1614138060"}, {"uid": "7534176617", "mid": "4608552906001487", "user_name": "伞何故", "text": "👴青回", "parent": "4608091582895681", "t": "1614248040"}, {"uid": "6497329749", "mid": "4608511919528309", "user_name": "滚滚蹲墙角", "text": "回复@莉莉安的仰望:不是未曾设想的路，是塞尔维亚曾经走过但被美国炸了的路", "parent": "4608505493855396", "t": "1614238320"}, {"uid": "2810498993", "mid": "4608505493855396", "user_name": "莉莉安的仰望", "text": "未曾设想的道路", "parent": "4608091582895681", "t": "1614236760"}, {"uid": "5610027649", "mid": "4608407586473214", "user_name": "恐龙记忆", "text": "回复@江北淮南def:给一个金托，银托也回不去了", "parent": "4608096204493530", "t": "1614213420"}, {"uid": "5773014643", "mid": "4608109655628088", "user_name": "吃一碗老陈醋", "text": "那就是回到曾经设想的道路了 鹰酱直呼大事不妙", "parent": "4608091582895681", "t": "1614142380"}, {"uid": "6900643037", "mid": "4608097299465658", "user_name": "苛刻可可66191", "text": "回复@江北淮南def:我们真的需要这样一个十年", "parent": "4608096204493530", "t": "1614139440"}, {"uid": "1888590214", "mid": "4608096204493530", "user_name": "江北淮南def", "text": "回复@滚滚蹲墙角:哦豁，又一个十年", "parent": "4608093059026828", "t": "1614139200"}, {"uid": "6497329749", "mid": "4608093059026828", "user_name": "滚滚蹲墙角", "text": "回复@江北淮南def:然后白头鹰又炸了", "parent": "4608092615218172", "t": "1614138420"}, {"uid": "1888590214", "mid": "4608092615218172", "user_name": "江北淮南def", "text": "他们需要下一个铁托", "parent": "4608091582895681", "t": "1614138300"}, {"uid": "7376012818", "mid": "4608091787895130", "user_name": "flosolis-", "text": "事实证明，跟中国🇨🇳 合作的都发财了，跟中国🇨🇳 作对的都自作自受了", "parent": "4608091134103360", "t": "1614138120"}, {"uid": "5785407039", "mid": "4608093206355535", "user_name": "yven_chen", "text": "这是被西方不当人久了，突然被兔子平等对待，感觉不可思议。", "parent": "4608091134103360", "t": "1614138480"}, {"uid": "6317228722", "mid": "4608173930190155", "user_name": "现实就是无法醒来的梦", "text": "突然发现可以不用跪着说话了。", "parent": "4608093206355535", "t": "1614157740"}, {"uid": "2207123751", "mid": "4608091650525113", "user_name": "进击的朱元璋", "text": "还做过美国梦，美国炸你们国家的时候你啥心情？", "parent": "4608091134103360", "t": "1614138120"}, {"uid": "5590470154", "mid": "4608626617223192", "user_name": "碇真嗣H", "text": "回复@炸芋粿:哎，麦克阿瑟重建日本非常成功... 直接变成美国附属", "parent": "4608334781481331", "t": "1614265620"}, {"uid": "6719853421", "mid": "4608615220256293", "user_name": "煑鹤焚琴", "text": "中国还不是一堆精日的，比如罗永浩", "parent": "4608091650525113", "t": "1614262920"}, {"uid": "2497358283", "mid": "4608584266024130", "user_name": "荆遇99", "text": "中国有美国梦的人不也多的很吗", "parent": "4608091650525113", "t": "1614255540"}, {"uid": "5609404341", "mid": "4608573503442555", "user_name": "鳖鳖宇", "text": "做过梦其实倒没什么，怕的是明明梦醒了，却还装作很感动，公知河山硕在美利坚感染新冠后临死前都不忘赞美美利坚", "parent": "4608091650525113", "t": "1614252960"}, {"uid": "6258832026", "mid": "4608417828179254", "user_name": "风起云扬洪波涌起", "text": "崇拜强者的天性，不得不承认，国内前些年有些人哪怕有轰炸南联盟大使馆、王海事件等，仍然憧憬美国梦，只不过更多的是选择自己跑到美国，享受所谓自由。只是近些年尤其疫情，更多人看清楚了本质。", "parent": "4608091650525113", "t": "1614215880"}, {"uid": "5149071230", "mid": "4608334781481331", "user_name": "炸芋粿", "text": "这是强者崇拜心态，就拿日本来说，唯一吃过原子弹的国家，结果无论战后到现在日本人最崇拜的都是美国", "parent": "4608091650525113", "t": "1614196080"}, {"uid": "2134878417", "mid": "4608312107336695", "user_name": "哥想去流浪", "text": "读小学的时候，我们老师说智利还是瑞士的人，18岁以后不想工作政府就发套房子，一个月给3000块当时我爹妈一个月也就几百块，房子是单位分配的没有买卖所以还不觉得，但是不用工作有钱拿还是让我对这些国家产生了很多期许，工作以后别人家的孩子也有出国留学然后就不回国了的，父母也很羡慕。直到近几年", "parent": "4608091650525113", "t": "1614190680"}, {"uid": "6461847374", "mid": "4608302204061748", "user_name": "鱼宝虎", "text": "醒了醒了啦，谁还没做过梦啊，我也做过", "parent": "4608091650525113", "t": "1614188280"}, {"uid": "5434800263", "mid": "4608239632650503", "user_name": "William2366", "text": "说实话二十年前美国实力如日中天有美国梦很正常，就连俄罗斯都一直对美国和欧盟抱有幻想", "parent": "4608091650525113", "t": "1614173400"}, {"uid": "6310538523", "mid": "4608207882292821", "user_name": "fffff777K", "text": "这话说的，还不准人年轻时有其他想法呢？ 你这话讲出去，搞得跟咱们和美国佬一个样", "parent": "4608091650525113", "t": "1614165780"}, {"uid": "1760767014", "mid": "4608099061603950", "user_name": "为啥微博不能改名啊卧槽", "text": "白猪炸他们的时候他应该已经不年轻了吧", "parent": "4608091650525113", "t": "1614139860"}, {"uid": "1915529831", "mid": "4608115650069745", "user_name": "张忆安-龙战于野", "text": "想想咱们中国人是怎么从中原部落联盟成长为华夏民族共同体的吧。中国带给世界的，一直是和平与可持续发展。", "parent": "4608091134103360", "t": "1614143820"}, {"uid": "5707227947", "mid": "4608775577666456", "user_name": "deprave25", "text": "回复@渣渣玉阳君:中国扩大版图的过程事实上是文化同化周围各民族和入侵者的过程。西方各国殖民的过程是先征服领土后统一语言，而你纵观我国历史却恰恰相反。", "parent": "4608234889941572", "t": "1614301140"}, {"uid": "5864282298", "mid": "4608640188678507", "user_name": "五音七弦琴", "text": "回复@渣渣玉阳君:西方可没有同化，只有宰掉。另外周之前史料里的真实细节太少，但是从秦汉推翻奴隶制开始，开疆拓土都是教化为主的，要不就是自卫反击。你可以了解一下朝贡体系和羁縻体系。和白人种族灭绝印第安然后现在再把人家圈养在哪里当猴子用。", "parent": "4608234889941572", "t": "1614268860"}, {"uid": "5746189585", "mid": "4608614700155802", "user_name": "给亚索加蓝条", "text": "回复@渣渣玉阳君:那为啥老祖宗打来打去，最后的结果是中国拥有了56个民族，而不像美利坚一样印第安人基本灭绝？", "parent": "4608234889941572", "t": "1614262800"}, {"uid": "6232000340", "mid": "4608600846894805", "user_name": "本地比较睿智的人之一", "text": "回复@渣渣玉阳君:您老祖宗是大和民族还是日耳曼民族啊", "parent": "4608234889941572", "t": "1614259500"}, {"uid": "7322301668", "mid": "4608518609177062", "user_name": "就是睿知呀", "text": "现代的确是这样，在古代高句丽，鲜卑，这些地方人眼里中原就像国人二十年前眼里的美国一样，有的及其羡慕或者说爱，有的极恨", "parent": "4608115650069745", "t": "1614239880"}, {"uid": "1133270014", "mid": "4608402301126700", "user_name": "强壮的猛男", "text": "回复@渣渣玉阳君:游牧民族不事生产，基本上都是在边境掠夺，老祖宗强大了就出去把他们打一顿顺便建立都护府之类的东西，然后土地就是我们的了", "parent": "4608234889941572", "t": "1614212160"}, {"uid": "6744981766", "mid": "4608250240308977", "user_name": "Always灬等待2018", "text": "回复@渣渣玉阳君:老祖宗貌似没有主动的侵略土地吧！基本上就是你不来弄我，我也不找你，你来打我，我就反打回去再同化你！", "parent": "4608234889941572", "t": "1614175920"}, {"uid": "7073017758", "mid": "4608246483257383", "user_name": "Asgardia02751", "text": "回复@渣渣玉阳君:你的话体现了你的无知", "parent": "4608234889941572", "t": "1614175020"}, {"uid": "2337311860", "mid": "4608234889941572", "user_name": "渣渣玉阳君", "text": "老祖宗不也是南征北战夺来的土地吗，夺来之后能同化的同化，不能的宰掉。", "parent": "4608115650069745", "t": "1614172260"}, {"uid": "6168339143", "mid": "4608096708331945", "user_name": "讨厌温情脉脉", "text": "中国人又何尝没做过美国梦呢？", "parent": "4608091134103360", "t": "1614139320"}, {"uid": "5593175911", "mid": "4608779855335925", "user_name": "你的遗照", "text": "回复@森林小兔子0206UK:有钱，在哪做梦都行。资本的世界", "parent": "4608767033606181", "t": "1614302160"}, {"uid": "1786063817", "mid": "4608767033606181", "user_name": "森林小兔子0206UK", "text": "回复@十万个冷笑话·:感觉并没有比中国好，也会有不干净的地方，也会有坏人。还没有深圳好呢。它并不比中国的月亮圆。我们团一起去参观的另一个团，被人砸了车窗