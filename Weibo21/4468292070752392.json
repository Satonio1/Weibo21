[{"uid": "1783258645", "mid": "4468292070752392", "user_name": "能歌哥Arenwong", "text": "广州南岸腾龙国际，女的在家里喷洒酒精消毒，男的在厨房做饭，家里就爆炸了，疫情防控期间，大家对酒精一定要安全使用 O网页链接", "parent": null, "t": "1580807280"}, {"uid": "2691001791", "mid": "4468292959431407", "user_name": "磨叽菌", "text": "辟谣了 赶紧删了吧", "parent": "4468292070752392", "t": "1580807520"}, {"uid": "2528718564", "mid": "4476583237081407", "user_name": "海月丫", "text": "回复@Nannnnnnncy:1", "parent": "4468481925505207", "t": "1582784040"}, {"uid": "1772334111", "mid": "4468481925505207", "user_name": "Nannnnnnncy", "text": "回复@磨叽菌:非常感谢", "parent": "4468313423695179", "t": "1580852520"}, {"uid": "2691001791", "mid": "4468313423695179", "user_name": "磨叽菌", "text": "回复 @Nannnnnnncy:微博搜酒精爆炸，好几个官媒都辟谣了。截图发给同事 啪啪打他们的脸", "parent": "4468292959431407", "t": "1580812380"}, {"uid": "1772334111", "mid": "4468310911084347", "user_name": "Nannnnnnncy", "text": "能不能指路告诉我哪里辟谣了？我是觉得是假的，但是我同事他们传得很开心居然不信我。", "parent": "4468292959431407", "t": "1580811780"}, {"uid": "3573415165", "mid": "4468315181039503", "user_name": "我好像是个沙雕", "text": "广州南岸？南岸区怕是重庆的", "parent": "4468292070752392", "t": "1580812800"}, {"uid": "2450286623", "mid": "4468324140641012", "user_name": "尔乃世之光棍佬", "text": "就是啊，广州南岸只是一条路……", "parent": "4468315181039503", "t": "1580814900"}, {"uid": "2483898943", "mid": "4469381201918158", "user_name": "季静与", "text": "硚口消防大队发布通报称，起火部位为武汉硚口团结佳兴园小区一屋内阳台，起火时室内无人居住，外来火种（烟头）引燃阳台杂物引发火灾。该住户未储存酒精等易燃易爆物品，现场爆炸声为燃烧引起家用火锅炉的小瓦斯罐爆炸，并非网传酒精消毒引发。", "parent": "4468292070752392", "t": "1581066960"}, {"uid": "6818742129", "mid": "4468315928288945", "user_name": "布莱恩特2008201011", "text": "转发微博", "parent": "4468292070752392", "t": "1580812980"}]