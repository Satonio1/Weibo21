[{"uid": "1455543965", "mid": "4480650641977503", "user_name": "京城老记", "text": "【《最悲惨的一生》主人公吴好运】男，1958年6月生于四川省北川县，出生的第二年中国发生了三年自然灾害，饿了三年肚子的他慢慢长大了，然而他的父母却死于饥饿，1976年，17岁的他考入了华北煤炭医学院（唐山），1976年7月27日他从四川到学校报到，第二天，发生了唐山大地震，他在倒塌的房子下幸运地活了下来，2003年调入广州市第一人民医院，刚刚上班，非典爆发，2008年5月10日，他回家乡北川祭祖，还没来得及走，遇上了汶川大地震，2019年1月，调入武汉市中心医院，2020年遇上了新冠肺炎，身为医生的他上了前线，1月27日被感染住院，2月23日病愈出院，前往福建泉州妻子处养病，入住欣佳宾馆隔离14天，3月7日晚，欣佳宾馆塌了，他没有躲过这次的追杀，享年62岁！", "parent": null, "t": "1583753820"}, {"uid": "6307780622", "mid": "4480652965072746", "user_name": "小妆美妆", "text": "nan", "parent": "4480650641977503", "t": "1583754360"}, {"uid": "1894839031", "mid": "4480652320136391", "user_name": "阿丽莎星座爱情运势", "text": "我是你的铁杆粉丝。", "parent": "4480650641977503", "t": "1583754180"}, {"uid": "7169084721", "mid": "4480651165716106", "user_name": "蓝子君", "text": "嗯呐，迟到的打卡签到", "parent": "4480650641977503", "t": "1583753940"}, {"uid": "5660364135", "mid": "4480650784429348", "user_name": "话兰州", "text": "我喜欢你啊 漫山遍野的喜欢。", "parent": "4480650641977503", "t": "1583753820"}, {"uid": "2456144587", "mid": "4480650670825472", "user_name": "小长春", "text": "nan", "parent": "4480650641977503", "t": "1583753820"}, {"uid": "5252233523", "mid": "4480661349734387", "user_name": "可曾听闻小羊肖恩", "text": "已经辟谣 ¡评论配图", "parent": "4480650641977503", "t": "1583756340"}, {"uid": "5404948272", "mid": "4480899192265577", "user_name": "kobunka", "text": "谣言，官方已经辟谣了。", "parent": "4480650641977503", "t": "1583813040"}, {"uid": "6331391017", "mid": "4480839809566190", "user_name": "名字过于强大而无法显示", "text": "官网已辟谣", "parent": "4480650641977503", "t": "1583798880"}, {"uid": "6144391109", "mid": "4480839444045189", "user_name": "喵小姐女郎", "text": "不是都已经澄清那是谣言吗", "parent": "4480650641977503", "t": "1583798820"}, {"uid": "3829464675", "mid": "4480801695987852", "user_name": "啥都是梦一月亮", "text": "不论真假，反正这段子让我记起这些灾难", "parent": "4480650641977503", "t": "1583789820"}, {"uid": "2299630585", "mid": "4480669121758440", "user_name": "czwyh", "text": "胡说八道", "parent": "448065064197750