[{"uid": "3488787807", "mid": "4440133166201664", "user_name": "今事", "text": "据香港网友爆料：新加坡已经取消香港人免签待遇了。", "parent": null, "t": "1574093700"}, {"uid": "1511582014", "mid": "4440133812231702", "user_name": "KENCHOICHOI", "text": "好吧，我这个两年无限次可以出来路过一下了。", "parent": "4440133166201664", "t": "1574093820"}, {"uid": "6491321817", "mid": "4440135376206988", "user_name": "毛小白喵", "text": "这个文件是“香港特别行政区签证身份书”，是由香港入境事务处签发给无法取得任何其他国家或地区的护照或旅行证件的香港居民的旅行证件。只有持这一证件的香港居民，才需要在来新加坡之前申请签证，持其他证件的香港居民，还是可以免签入境新加坡的！而且这一规定已经持续很久了，并不是最近才更改的。", "parent": "4440133166201664", "t": "1574094180"}, {"uid": "1629918684", "mid": "4440184068539328", "user_name": "5号地球人", "text": "李佳破瑟瑟发抖", "parent": "4440133166201664", "t": "1574105820"}, {"uid": "6090420134", "mid": "4440205580373530", "user_name": "海伦_fs", "text": "哈哈哈，恭喜你们盼望已久的人权自由终于到来了还搭上一些不支持你们的港人陪着你们，够面子的，开心吧？", "parent": "4440133166201664", "t": "1574110920"}, {"uid": "2686471001", "mid": "4440260953672030", "user_name": "Mecalamumu", "text": "所以啦，闹得结果自讨苦吃呀", "parent": "4440133166201664", "t": "1574124120"}, {"uid": "2683076544", "mid": "4440251151768971", "user_name": "兔子爱莲花", "text": "转发微博", "parent": "4440133166201664", "t": "1574121780"}, {"uid": "2637858187", "mid": "4440235909472693", "user_name": "煜涵她妈", "text": "转发微博", "parent": "4440133166201664", "t": "1574118180"}, {"uid": "2164838690", "mid": "4440232508006364", "user_name": "凹凸柚子酱", "text": "Repost", "parent": "4440133166201664", "t": "1574117340"}, {"uid": "244899296", "mid": "4440144897166571", "user_name": "AMOUR春", "text": "转发微博", "parent": "4440133166201664", "t": "1574096460"}, {"uid": "2603750361", "mid": "4440133442786710", "user_name": "想要早点退休的姜姜", "text": "转发微博", "parent": "4440133166201664", "t": "1574093760"}]