[{"uid": "1109137074", "mid": "4256236977251721", "user_name": "苏州花骑部落户外--雨琪1973", "text": "#合肥蹦极女孩# 传谣与辟谣\n一个吃瓜群众的自我救赎\n或者是一次深藏不漏的营销 L苏州花骑部落户外--雨琪1973的秒拍视频", "parent": null, "t": "1530249420"}, {"uid": "3031530823", "mid": "4258330807094387", "user_name": "Instyle头像馆集", "text": "你全家死了你知道吗", "parent": "4256236977251721", "t": "1530748620"}, {"uid": "1109137074", "mid": "4328402175405496", "user_name": "苏州花骑部落户外--雨琪1973", "text": "回复@繁华小布:请问你弄清楚了吗，人都有眼睛，就是没看到你的长到哪里去了", "parent": "4258330807094387", "t": "1547454900"}, {"uid": "1109137074", "mid": "4276455963695028", "user_name": "苏州花骑部落户外--雨琪1973", "text": "回复@繁华小布:是的，要弄弄清楚才不会冤枉好人，不是争口舌之快，只图真相而已", "parent": "4258330807094387", "t": "1535070000"}, {"uid": "3031530823", "mid": "4267915744756134", "user_name": "Instyle头像馆集", "text": "回复@苏州花骑部落户外--雨琪1973:自己没弄清楚就不要怪别人了，", "parent": "4264621421002376", "t": "1533033840"}, {"uid": "2725617707", "mid": "4264815545517585", "user_name": "Me大大大大大柚子", "text": "回复@苏州花骑部落户外--雨琪1973:社会需要正能量应该被鼓励，希望你下次传播正能量时，还能带着当初单纯的心，不被影响，一个传递一个，星星之火 可以燎原。", "parent": "4264621421002376", "t": "1532294700"}, {"uid": "1109137074", "mid": "4264621421002376", "user_name": "苏州花骑部落户外--雨琪1973", "text": "回复@繁华小布:您口中粉丝和我微博中的关注者不是一类人，他们不是脑残粉，而是在同一个人生观价值观圈子里有一样意识的人互相照应而已，没有人高高在上一呼百应，扭动世界滚轮的人也从来不是微博上呼风唤雨的人。可能眼睛看远一点，手指多点一下就能避免很多不必要的争吵", "parent": "4258330807094387", "t": "1532248440"}, {"uid": "1109137074", "mid": "4264620963107330", "user_name": "苏州花骑部落户外--雨琪1973", "text": "回复@繁华小布:如果您不是在喷我 也请您喷的时候署名带姓，免得我回嘴的时候误伤。连发两条微博怎么了？不是明星大碗怎么了？就不能辟谣了？就不能有人关注了？我的微博没表达清楚引起误会是我不对，因为我没有考虑到这样一条小微博会引来这么多脏话。", "parent": "4258330807094387", "t": "1532248320"}, {"uid": "1109137074", "mid": "4264618777981998", "user_name": "苏州花骑部落户外--雨琪1973", "text": "回复@Me大大大大大柚子:感谢理解，视频没有经过再次处理就直接上传会引起误会是我没有考虑到，浊者自浊清者自清，人群不同理解力也不同不用计较。毕竟世上不会有一件事能让所有人都认为是对的，还是感谢你的支持😊", "parent": "4263457018404624", "t": "1532247780"}, {"uid": "2725617707", "mid": "4263457018404624", "user_name": "Me大大大大大柚子", "text": "回复@繁华小布:您随意，开心就好", "parent": "4258330807094387", "t": "1531970820"}, {"uid": "3031530823", "mid": "4263456008468908", "user_name": "Instyle头像馆集", "text": "回复@Me大大大大大柚子:不对，你是清华毕业的，我是我奶奶交大的", "parent": "4263455512940167", "t": "1531970580"}, {"uid": "3031530823", "mid": "4263455563341344", "user_name": "Instyle头像馆集", "text": "回复@Me大大大大大柚子:实话实说，我一个人骂他，你可以找我麻烦。现在不是我一个人，他不是很出名的，一般人不会去看他微博的。", "parent": "4263455030150127", "t": "1531970460"}, {"uid": "2725617707", "mid": "4263455512940167", "user_name": "Me大大大大大柚子", "text": "回复@繁华小布:小女子不才，只是普通院校毕业，您说的都对，", "parent": "4258330807094387", "t": "1531970460"}, {"uid": "2725617707", "mid": "4263455030150127", "user_name": "Me大大大大大柚子", "text": "回复@繁华小布:好吧，你说的都对", "parent": "4258330807094387", "t": "1531970340"}, {"uid": "3031530823", "mid": "4263454653550783", "user_name": "Instyle头像馆集", "text": "回复@Me大大大大大柚子:自己不弄的明明白白，就算是误会你也怪不了别人。这种解释只有你这种高材生能理解，我们这种老百姓理解不了。", "parent": "4263453357340904", "t": "1531970220"}, {"uid": "3031530823", "mid": "4263454006812502", "user_name": "Instyle头像馆集", "text": "回复@Me大大大大大柚子:那你只能怪微博只让我们这些骂他的人看到这条，而不是看到两条", "parent": "4263453357340904", "t": "1531970100"}, {"uid": "3031530823", "mid": "4263453667393350", "user_name": "Instyle头像馆集", "text": "回复@Me大大大大大柚子:我刚看了一下解释在后面，这条微博就是没有解释，你以为是我一个人在骂他？", "parent": "4263453357340904", "t": "1531969980"}, {"uid": "2725617707", "mid": "4263453357340904", "user_name": "Me大大大大大柚子", "text": "原文字.传言与辟谣，或者是一次营销操作，作者并没有说什么，大家要注意了，玩蹦极太恐怖了.人没了，请转发啥的，也没有忽悠让别人转发，从字面意思，不难理解作者想表达的思想吧，很难吗", "parent": "4258330807094387", "t": "1531969920"}, {"uid": "3031530823", "mid": "4263453096847583", "user_name": "Instyle头像馆集", "text": "回复@Me大大大大大柚子:传谣与辟谣，然后下面还是原来的视频，是我脑子不好使，理解不了这意思？哪里解释了？", "parent": "4263452803269990", "t": "1531969860"}, {"uid": "2725617707", "mid": "4263452803269990", "user_name": "Me大大大大大柚子", "text": "回复@繁华小布:图片评论 ¡评论配图", "parent": "4258330807094387", "t": "1531969800"}, {"uid": "3031530823", "mid": "4263449591232449", "user_name": "Instyle头像馆集", "text": "回复@Me大大大大大柚子:我看的很清楚好吗？没有任何解释，你看到了？在哪里？", "parent": "4263