[{"uid": "2280198017", "mid": "4616412981629002", "user_name": "解放军报", "text": "【#朝鲜宣布与马来西亚断绝外交关系#】据朝中社19日报道，朝鲜外务省当天发表声明说，因马来西亚日前决定将一名朝鲜公民强行引渡到美国，朝鲜决定中断与马来西亚的外交关系。\n\n声明说，这名朝鲜公民多年来一直从事合法的外贸活动，马来西亚称其从事某种与“非法洗钱”相关的活动是一种荒谬的捏造和诡计。马来西亚当局17日将这名无辜的朝鲜公民当成“罪犯”，并将其强行引渡到美国，这种行为不可容忍。\n\n声明说，马来西亚当局此举破坏了两国关系中相互尊重主权的基础。鉴于事态的严重形势，朝鲜外务省宣布与“屈服美国强权而对朝鲜做出特大型敌对行为”的马来西亚完全断绝外交关系。\n\n声明最后说，从现在起马来西亚当局将要对双方之间可能发生的任何后果负全部责任。声明还警告美国作为这起事件的幕后黑手，也将付出代价。（新华社）", "parent": null, "t": "1616122080"}, {"uid": "5631841653", "mid": "4616413593735734", "user_name": "琅玕纸礼星子词", "text": "加拿大", "parent": "4616412981629002", "t": "1616122200"}, {"uid": "1390566463", "mid": "4616413447199738", "user_name": "赵锡功sdltzxg", "text": "朝鲜人民斗志昂扬！", "parent": "4616412981629002", "t": "1616122140"}, {"uid": "3296590031", "mid": "4616472591077935", "user_name": "一休班就不想早起", "text": "放弃幻想", "parent": "4616412981629002", "t": "1616136240"}, {"uid": "5656254879", "mid": "4616423873712951", "user_name": "外卖超人的没用徒弟", "text": "伟大的朝鲜必胜，伟大的无产阶级必胜", "parent": "4616412981629002", "t": "1616124660"}, {"uid": "1407282684", "mid": "4616440453797250", "user_name": "不念过去不忧未来", "text": "来两颗大棒", "parent": "4616412981629002", "t": "1616128620"}, {"uid": "3739116567", "mid": "4616431666467366", "user_name": "苍又嘉错", "text": "怎么回事？？？", "parent": "4616412981629002", "t": "1616126520"}, {"uid": "7268667640", "mid": "4616426008349876", "user_name": "北方拳民总教头", "text": "加拿大", "parent": "4616412981629002", "t": "1616125140"}, {"uid": "5939106466", "mid": "4616419989786760", "user_name": "逍遥子200405", "text": "nan", "parent": "4616412981629002", "t": "1616123700"}, {"uid": "7226153973", "mid": "4616416419908366", "user_name": "狐仙近卫军", "text": "孟晚舟要不是中国人，大加合可能早就把她引渡到美国🇺🇸 了", "parent": "4616412981629002", "t": "1616122860"}, {"uid": "5617639128", "mid": "4616413992191719", "user_name": "切葱拍蒜剁姜", "text": "马来西亚这浓眉大眼的家伙也开始边犬了吗", "parent": "4616412981629002", "t": "1616122320"}, {"uid": "3784647275", "mid": "4616413593208130", "user_name": "心安即是强大", "text"