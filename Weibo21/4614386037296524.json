[{"uid": "7270416771", "mid": "4614386037296524", "user_name": "宅姐说科技", "text": "腾讯公司的总部是在广东深圳，为什么马化腾却说腾讯的发源地是河北石家庄？\n谁会想到，距离深圳将近两千公里的石家庄，居然是腾讯的发源地。当然，这不是我杜撰的，而是马化腾自己说的。\n三年前的中国国际数字经济峰会上，马化腾亲口承认：石家庄就是腾讯的发源地。当年腾讯是在和河北电信公司进行网络传呼的合作，马化腾连着跑了四趟石家庄，腾讯公司第一桶金就是在石家庄获得的。\n可以说没有石家庄就没有腾讯的今天，而马化腾也是非常感恩石家庄。同样，如果没有马化腾的爆料，很少有人会知道腾讯和石家庄的渊源。\n\n#科技##数码#", "parent": null, "t": "1615638780"}, {"uid": "6835399831", "mid": "4614528791216599", "user_name": "宋宋文山", "text": "石家庄表示 我们不是复印机的发源地", "parent": "4614386037296524", "t": "1615672860"}, {"uid": "6348586123", "mid": "4615137828542056", "user_name": "做好自己就行_", "text": "你算老几啊？还代表", "parent": "4614528791216599", "t": "1615818060"}, {"uid": "6124192584", "mid": "4614998515780874", "user_name": "小田仙人", "text": "你能代表石家庄？", "parent": "4614528791216599", "t": "1615784820"}, {"uid": "5860444540", "mid": "4614631928369899", "user_name": "寻找那段失落的记忆", "text": "哈哈哈哈哈", "parent": "4614528791216599", "t": "1615697400"}, {"uid": "6835399831", "mid": "4614626001817847", "user_name": "宋宋文山", "text": "你家住海边呢？管那么宽", "parent": "4614528791216599", "t": "1615696020"}, {"uid": "6561953771", "mid": "4614596705124427", "user_name": "诸葛大大么么哒", "text": "你是石家庄?在这代表谁呢，", "parent": "4614528791216599", "t": "1615689000"}, {"uid": "1993507030", "mid": "4614750069852800", "user_name": "武汉艺术人声", "text": "坑害青少年的鼻祖，吸血青少年的魔鬼", "parent": "4614386037296524", "t": "1615725600"}, {"uid": "1993507030", "mid": "4617569158038733", "user_name": "武汉艺术人声", "text": "回复@小田仙人:你的格局是啥？就是专门助纣为虐，坑人害人吗？别装自己有多高尚，你不也就是那一粒微不足道的沙子", "parent": "4614998870986341", "t": "1616397720"}, {"uid": "6124192584", "mid": "4614998870986341", "user_name": "小田仙人", "text": "烟 酒  哪怕花盆害死人都比腾讯要多得多。人不能太没有格局", "parent": "4614750069852800", "t": "1615784880"}, {"uid": "6177519751", "mid": "4614995574261454", "user_name": "乐乐6177519751", "text": "给交点税吧", "parent": "4614386037296524", "t": "1615784100"}, {"uid": "5992939240", "mid": "4614664585221355", "user_name": "汤·氏", "text": "女神也是这么预料，不知道真实情况是如何。", "parent": "4614386037296524", "t": "1615705200"}]