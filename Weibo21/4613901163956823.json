[{"uid": "2885896690", "mid": "4613901163956823", "user_name": "最IN成都", "text": "#上海警方通报地铁站行李箱装孩子##女子嫌吵闹将5岁女儿塞行李箱#【上海警方通报地铁站行李箱装孩子：因母亲嫌5岁女儿吵闹 被民警制止】3月11日，上海轨交公安通报，民警在人民广场站发现一名女子将一名女孩放在行李箱内。民警立即拦截该女子并开展检查。经查，事发时32岁女子苏某急于赴上海火车站搭乘火车，因嫌其5岁女儿吵闹，将女儿放在行李箱内拖行，约1分钟后被民警拦停制止。经检查，女孩未受伤。事件正在进一步处置中。(新京报我们视频) L新京报我们视频的微博视频 2成都#成都热点关注#", "parent": null, "t": "1615523220"}, {"uid": "2886791004", "mid": "4613902246087983", "user_name": "成都生活播报", "text": "#女子嫌吵闹将5岁女儿塞行李箱#【上海警方通报地铁站行李箱装孩子：因母亲嫌5岁女儿吵闹 被民警制止】", "parent": "4613901163956823", "t": "1615523460"}, {"uid": "6890487951", "mid": "4614086446551157", "user_name": "呼延浩Eudora", "text": "真不是拐卖的吗？", "parent": "4613901163956823", "t": "1615567380"}, {"uid": "6029873171", "mid": "4614185843164767", "user_name": "丸常想一二", "text": "她是不是为了躲避买票啊", "parent": "4613901163956823", "t": "1615591080"}, {"uid": "5953298738", "mid": "4614099046237902", "user_name": "宽敞顾恋IUF9", "text": "呃呃平时就得好好教育，不然自尝苦果", "parent": "4613901163956823", "t": "1615570380"}, {"uid": "6696901282", "mid": "4614098728518186", "user_name": "声国兴丶干净", "text": "有可能窒息而死", "parent": "4613901163956823", "t": "1615570320"}, {"uid": "6890487951", "mid": "4614086446551202", "user_name": "呼延浩Eudora", "text": "真不是拐卖的吗？", "parent": "4613901163956823", "t": "1615567380"}, {"uid": "3127309800", "mid": "4613902669448890", "user_name": "成都圈子", "text": "#女子嫌吵闹将5岁女儿塞行李箱#【上海警方通报地铁站行李箱装孩子：因母亲嫌5岁女儿吵闹 被民警制止】", "parent": "4613901163956823", "t": "1615523580"}, {"uid": "2949911072", "mid": "4613902547031738", "user_name": "色食成都", "text": "#女子嫌吵闹将5岁女儿塞行李箱#【上海警方通报地铁站行李箱装孩子：因母亲嫌5岁女儿吵闹 被民警制止】", "parent": "4613901163956823", "t": "1615523520"}, {"uid": "2948703700", "mid": "4613902354614566", "user_name": "耍耍成都", "text": "#女子嫌吵闹将5岁女儿塞行李箱#【上海警方通报地铁站行李箱装孩子：因母亲嫌5岁女儿吵闹 被民警制止】", "parent": "4613901163956823", "t": "1615523460"}, {"uid": "2886791004", "mid": "4613902246088093", "user_name": "成都生活播报", "text": "#女子嫌吵闹将5岁女儿塞行李箱#【上海警方通报地铁站行李箱装孩子：因母亲嫌5岁女儿吵闹 被民警制止】", "parent": "4613901163956823", "t": "1615523460"}