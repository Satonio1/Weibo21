[{"uid": "2480479507", "mid": "4614652401287755", "user_name": "多啦圆梦Daisy", "text": "#如何看待教资热#\n\n考教资≠一定当老师≠一定考编制≠进得去体制内\n\n有人想考因为真的想当老师，但是高考失利没考上心仪的大学。\n有人想考因为家里觉得老师职业稳定。\n有人想考因为变化太多，想为以后的职业规划多一个选择的可能性。\n有人跟风因为别人都考了所以就去试试看。\n有人就是现在的职业不想做了，专业不喜欢了就业困难了想早早转行。\n\n换句话说，谁能保证以后的事，有备无患有错吗？每一个师范生就一定能保证自己一辈子不转行吗？不是吧。\n\n考的理由千千万，怎么管得这么宽？", "parent": null, "t": "1615702320"}, {"uid": "1655463335", "mid": "4614672622554731", "user_name": "杰悟Ginger", "text": "浪费资源", "parent": "4614652401287755", "t": "1615707120"}, {"uid": "1581023260", "mid": "4614953803976873", "user_name": "苏苏苏苏苏婉昭丶爱吃橙子", "text": "回复@叫我噗嗤123:我也看了。好紧张。我最近也在考虑要不要考教资，怎么办，我这个非师范的看的胆颤心惊，会不会也被这个法硕首长教育。瑟瑟发抖中。", "parent": "4614706356030447", "t": "1615774140"}, {"uid": "2703344571", "mid": "4614768956015007", "user_name": "是大福的福", "text": "我自己花钱自己考证，国家发证，浪费啥资源了呢？最多背书的时候总想点奶茶而已", "parent": "4614672622554731", "t": "1615730100"}, {"uid": "5122603338", "mid": "4614764523163208", "user_name": "永远18一枝花ctt", "text": "什么叫浪费资源  我靠自己的本事考教资  我能考过", "parent": "4614672622554731", "t": "1615729020"}, {"uid": "5828784321", "mid": "4614739381979039", "user_name": "April-ly", "text": "祝你以后永远不过教资让你多嘴", "parent": "4614672622554731", "t": "1615723020"}, {"uid": "6437569342", "mid": "4614722890500475", "user_name": "你怕是个傻子趴", "text": "不是吧，教资考试要交费的，人家巴不得越多的人考，报班买资料都是拉动内需", "parent": "4614672622554731", "t": "1615719120"}, {"uid": "1007204724", "mid": "4614716478197822", "user_name": "mizunn", "text": "这个怎么就浪费资源了？", "parent": "4614672622554731", "t": "1615717560"}, {"uid": "7358176059", "mid": "4614706356030447", "user_name": "叫我噗嗤123", "text": "回复 @快乐星球大酋长:好家伙，法硕应该不是你这样人能学的吧，眼界太浅，素质过低，真是水浅王八多，多张张见识吧", "parent": "4614672622554731", "t": "1615715160"}, {"uid": "2480479507", "mid": "4614704929964982", "user_name": "多啦圆梦Daisy", "text": "回复@Plivanje:无语子", "parent": "4614704729163365", "t": "1615714800"}, {"uid": "2823314393", "mid": "4614704858663040", "user_name": "Plivanje", "text": "回复@多啦圆梦Daisy:又来了一个 笑死 继续拉黑", "parent": "4614704929964982", "t": "1615714800"}, {"uid": "2823314393", "mid": "4614704729163365", "user_name": "Plivanje", "text": "回复@多啦圆梦Daisy:我也是 笑死 这就是他的嘴脸真看不起他连回复他都不屑", "parent": "4614704929964982", "t": "1615714800"}, {"uid": "6991564346", "mid": "4614704712389521", "user_name": "明日花绮小萝卜", "text": "回复 @多啦圆梦Daisy:shap两个", "parent": "4614672622554731", "t": "1615714800"}, {"uid": "6991564346", "mid": "4614704670443849", "user_name": "明日花绮小萝卜", "text": "回复 @Plivanje:算你爹", "parent": "4614672622554731", "t": "1615714740"}, {"uid": "2480479507", "mid": "4614704162669708", "user_name": "多啦圆梦Daisy", "text": "回复@Plivanje:哈哈哈他盯上我了还发私信", "parent": "4614703580712732", "t": "1615714620"}, {"uid": "2823314393", "mid": "4614704003287929", "user_name": "Plivanje", "text": "回复@多啦圆梦Daisy:你的微博你做主 他算个屁", "parent": "4614703840756483", "t": "1615714620"}, {"uid": "2480479507", "mid": "4614703840756483", "user_name": "多啦圆梦Daisy", "text": "回复@Plivanje:真不是我不许别人有反对意见，是他自视法硕贬低别人", "parent": "4614703580712732", "t": "1615714560"}, {"uid": "2823314393", "mid": "4614703655948754", "user_name": "Plivanje", "text": "回复@多啦圆梦Daisy:来一个拉黑一个", "parent": "4614703076349135", "t": "1615714500"}, {"uid": "2823314393", "mid": "4614703580712732", "user_name": "Plivanje", "text": "回复@阿姆斯特狼普:还换小号fw看不起你嘿嘿", "parent": "4614703454359953", "t": "1615714500"}, {"uid": "6737136228", "mid": "4614703530115286", "user_name": "阿姆斯特狼普", "text": "回复 @多啦圆梦Daisy:", "parent": "4614672622554731", "t": "1615714500"}, {"uid": "6737136228", "mid": "4614703487911711", "user_name": "阿姆斯特狼普", "text": "回复 @多啦圆梦Daisy:不，你没有", "parent": "4614672622554731", "t": "1615714500"}, {"uid": "6737136228", "mid": "4614703454359953", "user_name": "阿姆斯特狼普", "text": "回复 @多啦圆梦Daisy:啧啧，说不过的嘴脸啧啧阿里嘎多", "parent": "4614672622554731", "t": "1615714500"}, {"uid": "6737136228", "mid": "4614703391445689", "user_name": "阿姆斯特狼普", "text": "回复 @Plivanje:哈哈哈哈fw，这就急了？", "parent": "4614672622554731", "t": "1615714440"}, {"uid": "2823314393", "mid": "4614703370994386", "user_name": "Plivanje", "text": "回复@多啦圆梦Daisy:真看不惯他 以为自己很牛吗无语", "parent": "4614703076349135", "t": "1615714440"}, {"uid": "2480479507", "mid": "4614703076349135", "user_name": "多啦圆梦Daisy", "text": "回复@Plivanje:已经拉黑啦", "parent": "4614702753386706", "t": "1615714380"}, {"uid": "2480479507", "mid": "4614702879740315", "user_name": "多啦圆梦Daisy", "text": "回复@快乐星球大酋长:yoyoyo说不过你说不过你，惹不起惹不起 我平平无奇想当老师的门外汉哪儿吵得过素质低下的您啊", "parent": "4614702569098124", "t": "1615714320"}, {"uid": "2823314393", "mid": "4614702753386706", "user_name": "Plivanje", "text": "回复@多啦圆梦Daisy:你是博主，直接把这快乐星球拉黑禁言，跑到别人微博来撒野辱骂还很自豪了似的", "parent": "4614702422561565", "t": "1615714320"}, {"uid": "6171866232", "mid": "4614702569098124", "user_name": "快乐星球大酋长", "text": "回复 @多啦圆梦Daisy:是的（某人自己骂自己）", "parent": "4614672622554731", "t": "1615714260"}, {"uid": "2480479507", "mid": "4614702422561565", "user_name": "多啦圆梦Daisy", "text": "回复@快乐星球大酋长:6666毕竟微博门槛更低，什么人都能评论", "parent": "4614701982155564", "t": "1615714200"}, {"uid": "6171866232", "mid": "4614702312982075", "user_name": "快乐星球大酋长", "text": "回复 @多啦圆梦Daisy:这就人身攻击了？？666受迫害倾向挺严重，找个班上班", "parent": "4614672622554731", "t": "1615714200"}, {"uid": "2480479507", "mid": "4614702082821287", "user_name": "多啦圆梦Daisy", "text": "回复@快乐星球大酋长:人身攻击真不像法硕的素质🙊", "parent": "4614701713721139", "t": "1615714140"}, {"uid": "6171866232", "mid": "4614701982155564", "user_name": "快乐星球大酋长", "text": "回复 @多啦圆梦Daisy:毕竟教师门槛低，啥人都能进罢了", "parent": "4614672622554731", "t": "1615714140"}, {"uid": "2480479507", "mid": "4614701856067266", "user_name": "多啦圆梦Daisy", "text": "回复@快乐星球大酋长:我想当老师就乱了教育？那我可真厉害", "parent": "4614701444497937", "t": "1615714080"}, {"uid": "6171866232", "mid": "4614701713721139", "user_name": "快乐星球大酋长", "text": "回复 @多啦圆梦Daisy:教育乱象从你做起", "parent": "4614672622554731", "t": "1615714080"}, {"uid": "6171866232", "mid": "4614701582914057", "user_name": "快乐星球大酋长", "text": "回复 @多啦圆梦Daisy:确实没有，加油🐉", "parent": "4614672622554731", "t": "1615714020"}, {"uid": "6171866232", "mid": "4614701444497937", "user_name": "快乐星球大酋长", "text": "回复 @叫我噗嗤123:hhh笑死，我只是平平无奇的法硕罢了，笑笑教资考试乱象而已，教资考试不有手就能过吗，教师行业乱象就是从这开始蔓延的", "parent": "4614672622554731", "t": "1615714020"}, {"uid": "6816841023", "mid": "4614694175770706", "user_name": "就这样错过小王", "text": "不是吧。优胜劣汰。即使不需要的人考了证说明他具备这方面的能力。换言之真有点小本事。 考试人多也照样考得上", "parent": "4614672622554731", "t": "1615712280"}, {"uid": "2480479507", "mid": "4614691856846573", "user_name": "多啦圆梦Daisy", "text": "回复@叫我噗嗤123:明白人", "parent": "4614691550660984", "t": "1615711680"}, {"uid": "7358176059", "mid": "4614691550660984", "user_name": "叫我噗嗤123", "text": "回复 @快乐星球大酋长:我觉得你们好奇怪哦，连非师范生都考不过，也好惨，光想靠减少基数来过关，好像都不配说自己是师范生，还有人家自己的钱自己的努力，你们不努力，还水平不够，那怪谁呢（首先我是师范生，我有证）", "parent": "4614672622554731", "t": "1615711620"}, {"uid": "2480479507", "mid": "4614689521404492", "user_name": "多啦圆梦Daisy", "text": "回复@快乐星球大酋长:我是非师范考了是浪费你什么资源了吧 报名费我自己的钱 买书买资料花自己的钱用我自己的时间 请问跟你有什么关系", "parent": "4614684633730582", "t": "1615711140"}, {"uid": "1706168112", "mid": "4614686740318759", "user_name": "萝卜不是白色的", "text": "我寻思考试也有报名费啊，而且以后代课老师越来越多，对政府来说都是不赔本的买卖啊？", "parent": "4614672622554731", "t": "1615710480"}, {"uid": "6171866232", "mid": "4614684633730582", "user_name": "快乐星球大酋长", "text": "回复 @多啦圆梦Daisy:你就是其中一员是吧", "parent": "4614672622554731", "t": "1615710000"}, {"uid": "2480479507", "mid": "4614675016455289", "user_name": "多啦圆梦Daisy", "text": "是你提供的资源吗？浪费你的了吗", "parent": "4614672622554731", "t": "1615707720"}, {"uid": "5015034891", "mid": "4614666627583756", "user_name": "cuvuxgjbufuf", "text": "如果不做老师那其实没必要考，可以考自己需要的证", "parent": "4614652401287755", "t": "1615705680"}, {"uid": "5860536051", "mid": "4614926687535256", "user_name": "不知名的赵某", "text": "确实，那些考个教资都费劲吧啦的人，还想去当老师，笑死", "parent": "4614666627583756", "t": "1615767720"}, {"uid": "7416622490", "mid": "4614776288183793", "user_name": "言苟子o", "text": "Reply @多啦圆梦Daisy:教资以后大概会五年一审吧hhhhh", "parent": "4614666627583756", "t": "1615731840"}, {"uid": "5828784321", "mid": "4614739710445734", "user_name": "April-ly", "text": "你真管的宽，别人想考就考，能考过也是别人的本事，你管的着么你，最烦你这样的人", "parent": "4614666627583756", "t": "1615723140"}, {"uid": "1569730561", "mid": "4614695737362115", "user_name": "不滋年普洱擦", "text": "Reply @吧扎嘿扎:不是 当老师教师很香啊 虽然现在不想当老师 但是好歹有个可以退路", "parent": "4614666627583756", "t": "1615712640"}, {"uid": "2480479507", "mid": "4614679337372268", "user_name": "多啦圆梦Daisy", "text": "回复@吧扎嘿扎:你聪明 你嘴软 按你这个逻辑没天赋的人连努力都不配？", "parent": "4614677537753197", "t": "1615708740"}, {"uid": "5685379660", "mid": "4614677537753197", "user_name": "吧扎嘿扎", "text": "回复 @多啦圆梦Daisy:你笨如牛，嘴还硬如铁，按你这个逻辑，要为所有的可能性做准备，要考所有的资格证。", "parent": "4614666627583756", "t": "1615708320"}, {"uid": "2480479507", "mid": "4614674912381584", "user_name": "多啦圆梦Daisy", "text": "我现在不想 但不代表以后不想，谁能预知未来呢？谁又能保证一辈子没有做过后悔的决定", "parent": "4614666627583756", "t": "1615707660"}, {"uid": "1264035345", "mid": "4614675402590712", "user_name": "Th-cissi", "text": "换个思路，一个行业吸引了更多的人也就意味着吸引了更多的优秀人才呀。", "parent": "4614652401287755", "t": "1615707780"}, {"uid": "2038418210", "mid": "4614745283365971", "user_name": "过儿保佑逢考必过", "text": "回复@叫我噗嗤123:伤害性不大，侮辱性极强", "parent": "4614685003092361", "t": "1615724460"}, {"uid": "7358176059", "mid": "4614685003092361", "user_name": "叫我噗嗤123", "text": "对，连基础的证都拿不下来，光希望靠着减少基数过关呢，怕不是傻子吧", "parent": "4614675402590712", "t": "1615710060"}, {"uid": "2480479507", "mid": "4614675860554302", "user_name": "多啦圆梦Daisy", "text": "太同意了", "parent": "4614675402590712", "t": "1615707900"}, {"uid": "2768789235", "mid": "4614996547868180", "user_name": "1原来是刺猬吖", "text": "我来给加个热度", "parent": "4614652401287755", "t": "1615784340"}, {"uid": "2480479507", "mid": "4614997613743713", "user_name": "多啦圆梦Daisy", "text": "哈哈哈哈！！！", "parent": "4614996547868180", "t": "1615784580"}, {"uid": "6805080638", "mid": "4614679907798967", "user_name": "盐雎", "text": "没毛病，拿了证的不一定都会去当老师。考编什么都也都是自凭本事，有些人没必要嘴别人考了的", "parent": "4614652401287755", "t": "1615708860"}, {"uid": "2480479507", "mid": "4614682709328420", "user_name": "多啦圆梦Daisy", "text": "真的，我绝对不支持并不爱这个行业，但是跟风去考去当老师然后误人子弟的人，同样的，我也不支持因为专业去嘴非师范生考证的人，因为这些人里是有真正爱这个行业的", "parent": "4614679907798967", "t": "1615709520"}, {"uid": "6366729819", "mid": "4614721653964829", "user_name": "画舸庭娆", "text": "主要最讨厌的是把它当成后路巴拉巴拉的那种人。“以后找不着工作就去当老师”....?先不说老师是不是可以想当就当的，这种心态我觉得不是很可，毕竟是教书育人的工作，抱着这种心态不太好吧，本来教师行列里就有垃圾，还不少", "parent": "4614652401287755", "t": "1615718820"}, {"uid": "6366729819", "mid": "4614753076384429", "user_name": "画舸庭娆", "text": "回复@ccccc82664:是的。虽然他是一份可以养家糊口的工作，但说到底还是存在特殊性的。不能说要有多高尚的心态，但至少不能敷衍", "parent": "4614747847134773", "t": "1615726320"}, {"uid": "5774932804", "mid": "4614747847134773", "user_name": "ccccc82664", "text": "对。认同这种做法。特别是看到那些虐待儿童，打幼儿园孩子，还有对大点的孩子说话很过分的，教师的一些话真的能影响孩子的一生", "parent": "4614721653964829", "t": "1615725060"}, {"uid": "2480479507", "mid": "4614721703776844", "user_name": "多啦圆梦Daisy", "text": "是的 这种我也很讨厌！", "parent": "4614721653964829", "t": "1615718820"}, {"uid": "6120576387", "mid": "4614674706075339", "user_name": "木木呦呵", "text": "🙋想为以后的职业多一个选择，毕竟世事难料", "parent": "4614652401287755", "t": "1615707600"}, {"uid": "5232743821", "mid": "4614666443819743", "user_name": "Octust9", "text": "赞同！其实大家都是为了能多个选择，没有人选择的时候还在想是会不会耽误别人的选择别人的人生吧？那师范生去考别的证时，有没有想过你挡人家路了？不止是教资，考研考公也都越来越难越来越卷了，只能说这是发展的必然，这些选择符合目前大多数人的想法，而不是谁堵了谁的路。", "parent": "4614652401287755", "t": "1615705620"}, {"uid": "5021732403", "mid": "4614764397071794", "user_name": "wyn4869", "text": "我是想当老师，想考编制的", "parent": "4614652401287755", "t": "1615729020"}, {"uid": "5021732403", "mid": "4614765781193950", "user_name": "wyn4869", "text": "回复@多啦圆梦Daisy:学写，还是看命吧！！科二悬了，这次过了一定去曲阜拜一下先师孔圣人，允许我有可能继承他老人家的衣钵吧", "parent": "4614764615172544", "t": "1615729320"}, {"uid": "2480479507", "mid": "4614764615172544", "user_name": "多啦圆梦Daisy", "text": "加油加油 冲鸭", "parent": "4614764397071794", "t": "1615729080"}, {"uid": "7484246852", "mid": "4614756382281696", "user_name": "叼着奶瓶逛峡谷", "text": "现在考什么都热，宁夏西夏区考社区研究生报名三十几个，一共报了一两千人，别说其他的了", "parent": "4614652401287755", "t": "1615727100"}, {"uid": "2480479507", "mid": "4614756553982480", "user_name": "多啦圆梦Daisy", "text": "考公也是一样的", "parent": "4614756382281696", "t": "1615727160"}, {"uid": "6391019086", "mid": "4614755047966913", "user_name": "xiaotsany", "text": "这叫管的宽吗？这叫分析社会心理因素", "parent": "4614652401287755", "t": "1615726800"}, {"uid": "2480479507", "mid": "4614756474559399", "user_name": "多啦圆梦Daisy", "text": "理性分析当然没问题 但不理性的谩骂 攻击 侮辱 真的就是管得宽", "parent": "4614755047966913", "t": "1615727100"}, {"uid": "5974097494", "mid": "4614721255510577", "user_name": "DUAN_201607", "text": "是的 既然给了大众考教资的资格 就有存在的意义 各方都辩证看待吧", "parent": "4614652401287755", "t": "1615718700"}, {"uid": "2480479507", "mid": "4614721993180522", "user_name": "多啦圆梦Daisy", "text": "同意，有不同的意见很正常", "parent": "4614721255510577", "t": "1615718880"}, {"uid": "7544993217", "mid": "4614709854080558", "user_name": "双向奔赴才是真", "text": "同意！反正我的理想就是当个体育老师然后上课和学生打打闹闹，下课回家能有固定的时间发展我的爱好，", "parent": "4614652401287755", "t": "1615716000"}, {"uid": "2480479507", "mid": "4614711851352071", "user_name": "多啦圆梦Daisy", "text": "回复@双向奔赴才是真:你这么说我也觉得我不太配哈哈哈哈  我也是学的吃力的那种，但是不想放弃", "parent": "4614711603105873", "t": "1615716480"}, {"uid": "7544993217", "mid": "4614711603105873", "user_name": "双向奔赴才是真", "text": "回复@多啦圆梦Daisy:我也想当数学老师来着，但是俺不配，挺喜欢数学解题半天然后恍然大悟的感觉的", "parent": "4614710500003822", "t": "1615716420"}, {"uid": "2480479507", "mid": "4614710500003822", "user_name": "多啦圆梦Daisy", "text": "我小时候想当语文老师，后来高中对数学开窍了，就想当数学老师了，努力解决大家见数学就皱眉的问题！至少让我教的学生能不怕数学，哈哈哈", "parent": "4614709854080558", "t": "1615716180"}, {"uid": "7340472451", "mid": "4614660395106552", "user_name": "少年青衫1988", "text": "确实管的宽，又不是想当老师就能当上老师的。不得教育局发公告招聘教师吗，有多少需求，才会增加多少教师。大家都是凭本事考的", "parent": "4614652401287755", "t": "1615704180"}, {"uid": "7103905383", "mid": "4614703508621029", "user_name": "想摘星星的小胖儿", "text": "就是就是，各凭本事，那本来有那个能力为啥要管", "parent": "4614660395106552", "t": "1615714500"}, {"uid": "5609614580", "mid": "4614704368190718", "user_name": "米沃莉特", "text": "考公怎么没人吐槽", "parent": "4614652401287755", "t": "1615714680"}, {"uid": "5976052921", "mid": "4614720235504653", "user_name": "偷个橘子呀", "text": "还有的是妈妈逼迫考的，也不管孩子想干嘛", "parent": "4614652401287755", "t": "1615718460"}, {"uid": "3154939513", "mid": "4614762577531348", "user_name": "向日葵的竹子", "text": "也有被孩子逼迫的", "parent": "4614720235504653", "t": "1615728540"}, {"uid": "5976052921", "mid": "4614762724595495", "user_name": "偷个橘子呀", "text": "回复@向日葵的竹子:有故事的人😂", "parent": "4614762577531348", "t": "1615728600"}, {"uid": "5869506173", "mid": "4614656586420028", "user_name": "尔咚咚橙", "text": "这不是住在海边，是住在宇宙阿～什么都管", "parent": "4614652401287755", "t": "1615703280"}, {"uid": "2950024412", "mid": "4614685448736201", "user_name": "被圈养的野人", "text": "？考证热也有人说？那咋不说高考热？？反正上不上大学，就别考了呗", "parent": "4614652401287755", "t": "1615710180"}, {"uid": "5491727546", "mid": "4614723674576775", "user_name": "给我早睡听到了吗", "text": "没毛病！", "parent": "4614652401287755", "t": "1615719300"}, {"uid": "7172369639", "mid": "4614717224519649", "user_name": "honeybabya_", "text": "我是一点都不想考 被家里逼的", "parent": "4614652401287755", "t": "1615717740"}, {"uid": "5663227120", "mid": "4614716711768911", "user_name": "小疙瘩都会解开的Manny", "text": "考着玩玩儿", "parent": "4614652401287755", "t": "1615717620"}, {"uid": "2785947242", "mid": "4614715647721931", "user_name": "呆呆的胖熊x", "text": "还是想当老师 毕竟一年有三个月带薪休假 奈何好难上岸啊", "parent": "4614652401287755", "t": "1615717380"}, {"uid": "5339780246", "mid": "4614710060125368", "user_name": "飞哇光光", "text": "就是，我考什么关你屁事", "parent": "4614652401287755", "t": "1615716060"}, {"uid": "2923429845", "mid": "4614706297573625", "user_name": "樂喜o3o", "text": "我就是那个看别人都考自己也考着玩的那个", "parent": "4614652401287755", "t": "1615715160"}, {"uid": "5939121477", "mid": "4614663999065145", "user_name": "_加糖子薄荷", "text": "再加一条景点打折", "parent": "4614652401287755", "t": "1615705080"}, {"uid": "3807441598", "mid": "4614715836469727", "user_name": "到底该取什么昵称好呢", "text": "好多人都是因为父母呢，比如说我", "parent": "4614652401287755", "t": "1615717440"}, {"uid": "3175819705", "mid": "4614662723472140", "user_name": "五年识up一年别", "