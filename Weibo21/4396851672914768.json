[{"uid": "1579955845", "mid": "4396851672914768", "user_name": "环球震惊局", "text": "【艾滋病感染人数直线上升，尤其是高校彻底沦陷了】\n资讯点评：非洲黑人留学生和允许艾滋病人进入两项政策是造成艾滋病泛滥的主要原因！ #校园##刚果埃博拉疫情为全球卫生紧急事件##教育##留学生#", "parent": null, "t": "1563774540"}, {"uid": "7187690436", "mid": "4396957352739158", "user_name": "雅典娜1314", "text": "它娘的，也不知道是什么政策造成的，我们国家留学生去外国留学要花大价钱，非洲那边留学生过来留学，我们国家就挨贴钱，而且还带来了艾滋病，真心不懂为什么要这样", "parent": "4396851672914768", "t": "1563799740"}, {"uid": "7187690436", "mid": "4398333336335745", "user_name": "雅典娜1314", "text": "回复@心怀炽热:这样最好", "parent": "4397258600310311", "t": "1564127820"}, {"uid": "6021364384", "mid": "4397258600310311", "user_name": "心怀炽热", "text": "不是你不懂为什么要这样，而是中国国情就是这样，大国之心嘛，没想到会有这样的结果。这两年频频爆出外国留学生的违法和乱纪问题我觉着不出三年，国家会管控的，最起码外国留学生违法乱纪什么的，学校会严肃处理不会草草了之不然我也就对祖国失望了", "parent": "4396957352739158", "t": "1563871560"}, {"uid": "3170307317", "mid": "4397240753582364", "user_name": "GEFFIE_", "text": "满族当道", "parent": "4396957352739158", "t": "1563867300"}, {"uid": "3659829904", "mid": "4396877384199887", "user_name": "joking哥", "text": "山东大学功不可没。", "parent": "4396851672914768", "t": "1563780720"}, {"uid": "7031625619", "mid": "4397591691291028", "user_name": "响riyyy", "text": "回复@Bfx43:刚通", "parent": "4397487978503202", "t": "1563951000"}, {"uid": "6486836779", "mid": "4397487978503202", "user_name": "Bfx43", "text": "回复@响ruiyyy:村里3G?", "parent": "4396877384199887", "t": "1563926280"}, {"uid": "7031625619", "mid": "4397253810701214", "user_name": "响riyyy", "text": "睁开你狗眼看看图呐的最多", "parent": "4396877384199887", "t": "1563870420"}, {"uid": "1035479352", "mid": "4396859092761837", "user_name": "娱圈大表哥", "text": "赞博主", "parent": "4396851672914768", "t": "1563776340"}, {"uid": "5466332919", "mid": "4396883218395954", "user_name": "Forever_xtf", "text": "得感谢国家的好政策啊", "parent": "4396851672914768", "t": "1563782100"}, {"uid": "5534436567", "mid": "4396926059067300", "user_name": "无语修改昵称", "text": "想当第三世界的老大，这可能是必须付出的代价吧！政策制定者高高在上，苦了一众崇黑媚外的人", "parent": "4396851672914768", "t": "1563792300"}, {"uid": "3558160930", "mid": "4396954186717133", "user_name": "卞晓晓今天瘦了吗", "text": "有些女生就是这么没脑子  有些人就是洋垃圾  有些政策…嗯…", "parent": "4396851672914768", "t": "1563799020"}, {"uid": "3558160930", "mid": "4397274924977320", "user_name": "卞晓晓今天瘦了吗", "text": "回复@snowfreefree:我也是，简直呵呵了，居然还妄想有人勾引你，自我评价是不是有点高得癔症了？也不知道掂量掂量自己是个什么东西。", "parent": "4396954186717133", "t": "1563875460"}, {"uid": "3017367481", "mid": "4397272928303523", "user_name": "变态玩意怎么还没死", "text": "回复@卞晓晓今天瘦了吗:毕竟你这蛆虫太骚了，老是勾引我，你说你不嗡嗡我还嗡嗡你干嘛。", "parent": "4397270910770738", "t": "1563874980"}, {"uid": "3558160930", "mid": "4397270910770738", "user_name": "卞晓晓今天瘦了吗", "text": "回复@snowfreefree:对啊，我就是最好的，反正比你好呗。我也不想看呀，可是有只苍蝇一直在我这评论底下嗡嗡嗡，那我膈应的慌，我过来处理两下子还不行吗？", "parent": "4396954186717133", "t": "1563874500"}, {"uid": "3017367481", "mid": "4397270428547250", "user_name": "变态玩意怎么还没死", "text": "回复@卞晓晓今天瘦了吗:看不懂就看不懂呗，反正是别人的错，看懂了也是别人的错，我就是最美好的。", "parent": "4397268302082246", "t": "1563874380"}, {"uid": "3017367481", "mid": "4397269035929020", "user_name": "变态玩意怎么还没死", "text": "回复@卞晓晓今天瘦了吗:你不是滚了吗，又回来继续battle了，再用个半小时又说成几分钟，你又可以博学多才了。", "parent": "4397268302082246", "t": "1563874080"}, {"uid": "3558160930", "mid": "4397268302082246", "user_name": "卞晓晓今天瘦了吗", "text": "回复@snowfreefree:不好意思，你的那个病句太明显了，不用一个字一个字的抠，看一眼就知道有问题，小学生都能看出来有问题…而且我比你博学多才，不是靠显示的，本来就是，负责地告诉你，我不止比你博学多才，我还比你过得幸福。", "parent": "4396954186717133", "t": "1563873900"}, {"uid": "3017367481", "mid": "4397267475750964", "user_name": "变态玩意怎么还没死", "text": "回复@卞晓晓今天瘦了吗:你只用了几分钟来一个字一个字扣出来彰显自己多么博学多才？恼羞成怒了还能看得懂别人说什么？其实你只用总结一下，说我说的话你自己全看不懂，你只看得懂自己打得字不就得了。", "parent": "4397265332649451", "t": "1563873720"}, {"uid": "3558160930", "mid": "4397265332649451", "user_name": "卞晓晓今天瘦了吗", "text": "回复@snowfreefree:而且…姐姐，你看你的评论里面“另外一些别人”这是一种怎样的表达方式？？？我咋看不太懂呢，你找个人给你讲讲病句吧，我回复评论的这几分钟已经浪费我不少时间了，再见", "parent": "4396954186717133", "t": "1563873180"}, {"uid": "3558160930", "mid": "4397263881223100", "user_name": "卞晓晓今天瘦了吗", "text": "回复@snowfreefree:我用着您来教我把“有些女生”改成“有些人”了？您这语文水平管好自己吧先呵呵呵9012年了，还有人讨论重男轻女的问题，不好意思，我接触到的人里面，从来没有重男轻女的，我们的老一辈也没有，所以我从来没有关于重男轻女的阴影，更没有“歧视”这种消极字眼", "parent": "4396954186717133", "t": "1563872820"}, {"uid": "3558160930", "mid": "4397262988302184", "user_name": "卞晓晓今天瘦了吗", "text": "回复@snowfreefree:我建议您重新回去读小学不用跟我说大道理，也不用迂回话语避重就轻，您不用在这里装正义之士，反驳？呵呵呵显示你的思想清高吗？我就是这么说怎么着！！说的人明明没有这个意思，而且大部分人都没有这个意思，偏偏有些人现在道德点上高谈阔论，我们的思想都病态，您个人清高行不行？？告辞", "parent": "4396954186717133", "t": "1563872640"}, {"uid": "3017367481", "mid": "4397260987197245", "user_name": "变态玩意怎么还没死", "text": "回复@卞晓晓今天瘦了吗:有些没问题，而是有些女生有问题，你改成有些人就没问题了。我说的别人歧视你这里的你也不是你，而是专指一类人，譬如重男轻女的女性，那句话是个总结。被反驳了就只会说别人无脑，呵。", "parent": "4397260299132782", "t": "1563872160"}, {"uid": "3558160930", "mid": "4397260299132782", "user_name": "卞晓晓今天瘦了吗", "text": "回复@snowfreefree:您在我客观的评论抒发这种观点，所以您是闲着没事儿找骂，还是我无意间顶撞您了？？？", "parent": "4396954186717133", "t": "1563871980"}, {"uid": "3558160930", "mid": "4397259716398189", "user_name": "卞晓晓今天瘦了吗", "text": "大家随口乱喷人的时候看好了哦“有些”！！！我希望大家都是有素质并且语文水平过关的", "parent": "4396954186717133", "t": "1563871860"}, {"uid": "3558160930", "mid": "4397257975376547", "user_name": "卞晓晓今天瘦了吗", "text": "回复@snowfreefree:得了，我算知道无脑喷子是啥了…以后话都不敢说了行了吧，服了别人歧视我？您真搞笑，您怕不是心理有啥疾病吧，“有些有些”您不识字？？？", "parent": "4396954186717133", "t": "1563871440"}, {"uid": "3017367481", "mid": "4397249239158533", "user_name": "变态玩意怎么还没死", "text": "看了一下你也是女生，你让我想起了那些重男轻女的女人。别人歧视你，你就加倍的歧视另外一些别人。", "parent": "4396954186717133", "t": "1563869340"}, {"uid": "3017367481", "mid": "4397248110848931", "user_name": "变态玩意怎么还没死", "text": "上面写的过去一年3077名学生感染艾滋，81.8%是同性性行为导致的。所以你为什么要把女生单独拎出来。", "parent": "4396954186717133", "t": "1563869100"}, {"uid": "2618499873", "mid": "4397062516994946", "user_name": "传捷958_449", "text": "我们崇拜的象牙塔，居然培养了90%艾滋，匪夷所思，功不可没", "parent": "4396851672914768", "t": "1563824820"}, {"uid": "1301297680", "mid": "4397177335677381", "user_name": "叫谷子", "text": "一方面是教育机制出了问题（特别山大的领导们），一方面是我们（特别山大的女生）更应该洁身自好，不要为了一点点利益而毁了自己一辈子。如果是被逼无奈情况下，希望你们能够勇敢面对，将逼迫你们的人（畜牲）告发出来，不能向恶势力低头。", "parent": "4396851672914768", "t": "1563852180"}, {"uid": "1415267254", "mid": "4397116048792155", "user_name": "麦田风继续", "text": "这些女生简直不知自爱 找黑猩猩不以为耻 反以为荣", "parent": "4396851672914768", "t": "1563837600"}, {"uid": "3558160930", "mid": "4397261628668894", "user_name": "卞晓晓今天瘦了吗", "text": "姐姐明白人…我真是太心累了，我来找姐姐诉苦来了…发了个评论被一个自己吃不饱还狗抓耗子的给喷… ¡评论配图", "parent": "4397116048792155", "t": "1563872280"}, {"uid": "6758608017", "mid": "4397126236504784", "user_name": "惘遠方03895", "text": "这", "parent": "4396851672914768", "t": "1563840000"}, {"uid": "1078637550", "mid": "4396966387242713", "user_name": "LiYa菲菲", "text": "中国人还是太多，尤其中产太多，还需要再计划掉一波", "parent": "4396851672914768", "t": "1563801900"}, {"uid": "1492540423", "mid": "4397159291850767", "user_name": "沒有co姐看就要窒息了的nanako", "text": "这政策……以后还是长点心吧", "parent": "4396851672914768", "t": "1563847920"}, {"uid": "6081727666", "mid": "4396936276386662", "user_name": "沧海一声笑的小心情", "text": "这帮女的是不是傻，操", "parent": "4396851672914768", "t": "1563794760"}, {"uid": "5823114440", "mid": "4397886902913869", "user_name": "肥肥小珂", "text": "看见没，此消息为不实消息，已处理，营销号就是天天放屁的狗", "parent": "4396851672914768", "t": "1564021380"}, {"uid": "5704169275", "mid": "4397261041578068", "user_name": "要来一口草莓牛奶吗", "text": "为什么入境前不检查检查呢 还有为什么女生会喜欢黑人做男朋友？难道不觉得看着就很害怕吗", "parent": "4396851672914768", "t": "1563872160"}, {"uid": "5873132084", "mid": "4397254456172221", "user_name": "固执d年轮", "text": "这会不会是控制人口的政策", "parent": "4396851672914768", "t": "1563870600"}, {"uid": "2417694627", "mid": "4397250815478531", "user_name": "叽里咕噜梆梆锤", "text": "社会主义好呀(●°u°●) 」", "parent": "4396851672914768", "t": "1563869700"}, {"uid": "3256019000", "mid": "4397247619423382", "user_name": "天哪起个破名真费劲", "text": "说这些有什么用？？？国家不管理，谁也没办法，说多了容易被抓，能怎么办，找女朋友注意点呗，接盘侠的日子不好过咯", "parent": "4396851672914768", "t": "1563868980"}, {"uid": "3910968771", "mid": "4397247481846764", "user_name": "吾皇阿朱", "text": "脏黑", "parent": "4396851672914768", "t": "1563868920"}, {"uid": "1160693352", "mid": "4397247447578189", "user_name": "五光十色镶钻V", "tex