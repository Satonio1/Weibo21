[{"uid": "2885395354", "mid": "4613901037344870", "user_name": "成都全城搜罗", "text": "#上海警方通报地铁站行李箱装孩子##女子嫌吵闹将5岁女儿塞行李箱#【上海警方通报地铁站行李箱装孩子：因母亲嫌5岁女儿吵闹 被民警制止】3月11日，上海轨交公安通报，民警在人民广场站发现一名女子将一名女孩放在行李箱内。民警立即拦截该女子并开展检查。经查，事发时32岁女子苏某急于赴上海火车站搭乘火车，因嫌其5岁女儿吵闹，将女儿放在行李箱内拖行，约1分钟后被民警拦停制止。经检查，女孩未受伤。事件正在进一步处置中。(新京报我们视频) L新京报我们视频的微博视频 2成都#成都热点关注#", "parent": null, "t": "1615523160"}, {"uid": "2159781124", "mid": "4613901864404949", "user_name": "我爱休闲成都", "text": "#女子嫌吵闹将5岁女儿塞行李箱#【上海警方通报地铁站行李箱装孩子：因母亲嫌5岁女儿吵闹 被民警制止】", "parent": "4613901037344870", "t": "1615523340"}, {"uid": "6693297793", "mid": "4614300456454859", "user_name": "中照捷丶年轻", "text": "会缺氧闷死吗？这当妈的也太无语了", "parent": "4613901037344870", "t": "1615618380"}, {"uid": "6892785968", "mid": "4614256488876332", "user_name": "辟开畅丶经纪人", "text": "熊孩子加熊家长", "parent": "4613901037344870", "t": "1615607880"}, {"uid": "6891188463", "mid": "4613938219585251", "user_name": "首高懿丶壮实", "text": "那也不能这样装啊，这妈不成熟", "parent": "4613901037344870", "t": "1615532040"}, {"uid": "6386064739", "mid": "4614092076090896", "user_name": "让永怡Hale", "text": "把孩子放行李箱里，那行李箱里原来的东西呢？出远门不可能带个空箱子吧？有东西的话还能把这么大一孩子放进去？", "parent": "4613901037344870", "t": "1615568700"}, {"uid": "2830437044", "mid": "4613901972407661", "user_name": "成都排行榜", "text": "#女子嫌吵闹将5岁女儿塞行李箱#【上海警方通报地铁站行李箱装孩子：因母亲嫌5岁女儿吵闹 被民警制止】", "parent": "4613901037344870", "t": "1615523400"}, {"uid": "6693297793", "mid": "4614300456454898", "user_name": "中照捷丶年轻", "text": "会缺氧闷死吗？这当妈的也太无语了", "parent": "4613901037344870", "t": "1615618380"}, {"uid": "6892785968", "mid": "4614256488876577", "user_name": "辟开畅丶经纪人", "text": "熊孩子加熊家长", "parent": "4613901037344870", "t": "1615607880"}, {"uid": "6889970081", "mid": "4614227974949385", "user_name": "往如丶3E", "text": "哎，还有这样的妈呀，", "parent": "4613901037344870", "t": "1615601100"}, {"uid": "6386064739", "mid": "4614092076090932", "user_name": "让永怡Hale", "text": "把孩子放行李箱里，那行李箱里原来的东西呢？出远门不可能带个空箱子吧？有东西的话还能把这么大一孩子放进去？", "parent": "4613901037344870", "t": "1615568700"}, {"uid": "7551817599", "