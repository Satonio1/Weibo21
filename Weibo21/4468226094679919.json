[{"uid": "1950292293", "mid": "4468226094679919", "user_name": "莲子豆豆", "text": "#兴趣话题词#   #抗疫新药用于临床试验#   #入组病人270名#\n2月2日，中国国家药监局药品审评中心（CDE）正式受理瑞德西韦的临床试验申请。另据外媒报道，美国公共安全卫生部门经特朗普特批，同意将该药物专利豁免，向中国紧急公开药物分子结构至4月27日。\n按计划，试验将从2月3日开始，总样本量270例，入组轻、中度新冠肺炎患者。这次，我国将启动的临床试验是III期、随机、双盲、安慰剂对照试验，旨在确定使用瑞德西韦治疗2019-nCoV的安全性和有效性。据了解，试验由中日友好医院教授曹彬牵头，预期2月3日开始，4月27日结束。", "parent": null, "t": "1580791560"}, {"uid": "1969485223", "mid": "4468246600555793", "user_name": "是酱变美的", "text": "下午好呀", "parent": "4468226094679919", "t": "1580796420"}, {"uid": "5318876003", "mid": "4468235066191358", "user_name": "你吖别妆了", "text": "豆豆", "parent": "4468226094679919", "t": "1580793660"}, {"uid": "6779417439", "mid": "4468227298267871", "user_name": "搞笑勇哥", "text": "nan", "parent": "4468226094679919", "t": "1580791860"}, {"uid": "6168525433", "mid": "4468226912200163", "user_name": "隐身丶忄", "text": "来你这里看看，有没有人喜欢我，没有的话，等下我再来问问！？", "parent": "4468226094679919", "t": "1580791740"}, {"uid": "1997741165", "mid": "4468226782203209", "user_name": "电影大爷", "text": "中午好", "parent": "4468226094679919", "t": "1580791740"}, {"uid": "2162414377", "mid": "4468226618546114", "user_name": "为什么不来问可爱颜诗女", "text": "中午好", "parent": "4468226094679919", "t": "1580791680"}, {"uid": "6272988791", "mid": "4468226568054314", "user_name": "Korea-美少女", "text": "中午好", "parent": "4468226094679919", "t": "1580791680"}, {"uid": "1899156270", "mid": "4468226425868730", "user_name": "icon星时尚", "text": "nan", "parent": "4468226094679919", "t": "1580791620"}, {"uid": "5532200151", "mid": "4468226169274451", "user_name": "Insipid伍Sir", "text": "愿2020......", "parent": "4468226094679919", "t": "1580791560"}, {"uid": "5850506328", "mid": "4468226115721772", "user_name": "每日论坛", "text": "中午好", "parent": "4468226094679919", "t": "1580791560"}, {"uid": "7346944372", "mid": "4468226110955322", "user_name": "每日军视", "text": "中午好", "parent": "4468226094679919", "t": "1580791560"}, {"uid": "2257175873", "mid": "4468490646076375", "user_name": "热门微博"