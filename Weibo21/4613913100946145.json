[{"uid": "2298474433", "mid": "4613913100946145", "user_name": "沈阳最头条", "text": "【#沈阳# 沈阳一小区门前两车迎面相撞，宝马车胎被撞脱落】3月11日上午9点左右，大东区考场街山水文园区西门口，一辆宝马越野车与一辆现代轿车相撞，造成严重车损，一人受伤。据目击者称，当时宝马车由北向南行驶，过程中突然驶入逆向车道，与对向一台轿车迎头相撞。事故发生后，宝马越野车左侧车头严重破损、车轮爆胎轮毂破碎，车内6个气囊全部弹出；与其相撞的轿车左侧保险杠、翼子板及前大灯破碎，车轮也爆了胎，轿车司机受伤被120拉走送医救治。据越野车司机介绍，当时他行驶路过小区门口时，突遇两个老太太横过马路，为躲避行人，他才驶入逆向车道，结果与对向驶来的轿车撞在了一块，他愿意承担事故的全部责任。2沈阳 #沈阳爆料#", "parent": null, "t": "1615526040"}, {"uid": "2476481021", "mid": "4613915977193125", "user_name": "小逗比神吐槽", "text": "撞得好严重", "parent": "4613913100946145", "t": "1615526700"}, {"uid": "2300220545", "mid": "4613915915062090", "user_name": "电影料", "text": "撞得好严重", "parent": "4613913100946145", "t": "1615526700"}, {"uid": "2300568541", "mid": "4613915948617955", "user_name": "禁止曝光", "text": "撞得好严重", "parent": "4613913100946145", "t": "1615526700"}, {"uid": "1454981493", "mid": "4614920757051741", "user_name": "苦中作乐2020", "text": "让速不让路。", "parent": "4613913100946145", "t": "1615766280"}, {"uid": "5665775151", "mid": "4614154487070727", "user_name": "宝海云天", "text": "唉，行人也要遵守交通规则啊", "parent": "4613913100946145", "t": "1615583580"}, {"uid": "7374081787", "mid": "4613969363339035", "user_name": "MY小马儿", "text": "咋不撞死 那两个老太太？", "parent": "4613913100946145", "t": "1615539480"}, {"uid": "1810223947", "mid": "4613947111506994", "user_name": "夏栀爱大茉", "text": "宝马车速过快 那条小路很窄 多有弯道盲区 车速控制的好就能刹停了", "parent": "4613913100946145", "t": "1615534140"}, {"uid": "6848857096", "mid": "4614125809828759", "user_name": "碎花丶5Z", "text": "秋天，乡村吃烧鱼去", "parent": "4613913100946145", "t": "1615576740"}, {"uid": "1408567450", "mid": "4613992566492377", "user_name": "rose99", "text": "宝马车你是不是开的有点快了呢？", "parent": "4613913100946145", "t": "1615545000"}, {"uid": "2300176877", "mid": "4613919786928691", "user_name": "美妆技", "text": "", "parent": "4613913100946145", "t": "1615527660"}, {"uid": "2302216925", "mid": "4613919786928592", "user_name": "giaogiao的猪八戒", "text": "", "parent": "4613913100946145", "t": "1615527660"}, {"uid":