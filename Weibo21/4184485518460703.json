[{"uid": "5621650438", "mid": "4184485518460703", "user_name": "妤姐不看Si信", "text": "给孩子积德吧吉林省松原市深井子镇，养殖狐狸老板和老婆去打麻将 ，两个孩子饿了在家做饭 ，煤气罐爆炸 ，两个孩子都被炸死 现场哭得嘶声力竭，他们家后院堆积的都是他们活活扒皮的狐狸尸体。因果轮回早与晚种什么因得什么果缺什么别缺德 L美拍（通过 美拍 拍摄）", "parent": null, "t": "1513142520"}, {"uid": "5812169479", "mid": "4184629986085241", "user_name": "l花袭人", "text": "博主三观不正！该批判的是夫妻顾自打麻将，而忽略在家孩子的安危！扯什么因果报应！", "parent": "4184485518460703", "t": "1513176960"}, {"uid": "2847591201", "mid": "4186820147281746", "user_name": "小汪喵234", "text": "可能就是报应吧", "parent": "4184629986085241", "t": "1513699140"}, {"uid": "5621650438", "mid": "4185440057239710", "user_name": "妤姐不看Si信", "text": "回复@污奈天真:哈哈没关系救用这个、为啥要换不要在乎那种龌龊的人说什么", "parent": "4184629986085241", "t": "1513370100"}, {"uid": "5621650438", "mid": "4185439382891202", "user_name": "妤姐不看Si信", "text": "回复@污奈天真:估计她看到我相册里有一张照片和你的头像是一样的了", "parent": "4184629986085241", "t": "1513369920"}, {"uid": "5621650438", "mid": "4184953945896323", "user_name": "妤姐不看Si信", "text": "回复@l花袭人:嗯请便在回你你都是我生的", "parent": "4184629986085241", "t": "1513254180"}, {"uid": "5621650438", "mid": "4184953414198577", "user_name": "妤姐不看Si信", "text": "回复@l花袭人:嗯对、我这些小号都是我自己自编自导自演你想怎么说就怎么说毕竟你自己的嘴", "parent": "4184629986085241", "t": "1513254060"}, {"uid": "5621650438", "mid": "4184887114050676", "user_name": "妤姐不看Si信", "text": "回复@l花袭人:我没那么闲还弄几个号、你以为别人都是你我都懒得搭理你了", "parent": "4184629986085241", "t": "1513238280"}, {"uid": "5621650438", "mid": "4184656163190939", "user_name": "妤姐不看Si信", "text": "回复@囚云呜呼:哈哈哈妇道是讲已婚妇女、你老娘我还没结婚、你连妇道是啥都不懂还在这装、说出来笑掉大牙、土鳖、你小学没读过吗", "parent": "4184629986085241", "t": "1513183200"}, {"uid": "5621650438", "mid": "4184649233697501", "user_name": "妤姐不看Si信", "text": "回复@囚云呜呼:你说我不遵守妇道、那你和我讲解一下什么是妇道？", "parent": "4184629986085241", "t": "1513181580"}, {"uid": "5621650438", "mid": "4184649066425185", "user_name": "妤姐不看Si信", "text": "回复@囚云呜呼:你来和我说说啥是妇道？", "parent": "4184629986085241", "t": "1513181520"}, {"uid": "5621650438", "mid": "4184648777119221", "user_name": "妤姐不看Si信", "text": "回复