[{"uid": "3829572249", "mid": "4270038053341036", "user_name": "z朱长艳", "text": "常州礼嘉工业园一工厂毒气泄露，目测嗅测是二氯二乙硫醚（俗称芥子气，日本二战毒气弹的一种），政府已经组织附近人员紧急撤离。过往青阳路礼嘉段的车辆尽量绕行。 2常州·吾悦广场", "parent": null, "t": "1533539820"}, {"uid": "6334801000", "mid": "4270050988934899", "user_name": "用户6334801000", "text": "#警方通报# 【关于“常州市嘉润水处理有限公司废酸储罐泄漏事件”的情况说明】   2018年8月6日上午9点40分左右，位于礼嘉镇的常州市嘉润水处理有限公司发生废酸储罐泄漏。事件发生后，市区镇三级领导及公安、应急、消防、安监、环保等相关部门立即赶赴现场处理，", "parent": "4270038053341036", "t": "1533542940"}, {"uid": "3829572249", "mid": "4270069871612884", "user_name": "z朱长艳", "text": "搞什么。我只是看到朋友圈发的。提醒大家注意而已。说话能别那么难听吗……什么鬼。你有脑子 就可以随便说别人没脑子吗🧠。同样是9年义务教育。你怎么那么优秀", "parent": "4270050988934899", "t": "1533547440"}, {"uid": "6347018883", "mid": "4270040561958765", "user_name": "JSCZWuSheng", "text": "嗅测后的你还好吗？", "parent": "4270038053341036", "t": "1533540420"}, {"uid": "5604767724", "mid": "4365618998368243", "user_name": "四大名捕1221", "text": "假的吧", "parent": "4270038053341036", "t": "1556328120"}, {"uid": "6334801000", "mid": "4270051442140097", "user_name": "用户6334801000", "text": "是芥子气的话还得了，不动脑子", "parent": "4270038053341036", "t": "1533543060"}, {"uid": "6334801000", "mid": "4270051156184151", "user_name": "用户6334801000", "text": "谣言止于智者", "parent": "4270038053341036", "t": "1533542940"}, {"uid": "2006428831", "mid": "4270048665504594", "user_name": "一缕光盲", "text": "皇塘地区会不会有影响", "parent": "4270038053341036", "t": "1533542340"}, {"uid": "2006428831", "mid": "4270048384378038", "user_name": "一缕光盲", "text": "往哪个方向飘的情况有没有得到控制", "parent": "4270038053341036", "t": "1533542280"}, {"uid": "2006428831", "mid": "4270048245697261", "user_name": "一缕光盲", "text": "我想知道如何预防很近距离只有十公里会不会有影响", "parent": "4270038053341036", "t": "1533542280"}, {"uid": "6624651116", "mid": "4270044210921362", "user_name": "用户6624651116", "text": "芥子气对皮肤伤害巨大切有很强的穿透力，普通的大衣都无法抵挡毒气对皮肤的伤害，为图一的民警同志揪心", "parent": "4270038053341036", "t": "1533541320"}]