[{"uid": "3370388762", "mid": "4614382678705143", "user_name": "业余宽客", "text": "由于病毒经济停摆，政府直接往个人卡里打钱，再加上没地方花钱，各国的储蓄都出现了一个爆发式的增长，仅美国一家就增长了2万亿（中国由于没有直接打钱增长并不像其他国家那么明显）。这种级别的天量储蓄病毒过后一旦释放势必将带来全球报复性消费，如此一来上游大宗商品的需求必然也将迎来一个爆发式的增长。#经济##宏观经济#", "parent": null, "t": "1615638000"}, {"uid": "2341471831", "mid": "4614463401495018", "user_name": "通信老兵2006", "text": "大宗已经爆发过，还追吗？", "parent": "4614382678705143", "t": "1615657260"}, {"uid": "3370388762", "mid": "4615006606328944", "user_name": "业余宽客", "text": "黑黑兄弟应该还有空间", "parent": "4614463401495018", "t": "1615786740"}, {"uid": "1678071243", "mid": "4614631296077055", "user_name": "我爱高电压", "text": "贬值太厉害了", "parent": "4614382678705143", "t": "1615697280"}, {"uid": "3370388762", "mid": "4615006984081905", "user_name": "业余宽客", "text": "nan", "parent": "4614631296077055", "t": "1615786860"}, {"uid": "1283011544", "mid": "4614986418356413", "user_name": "金芒果星闻社", "text": "就套这些思维慢半拍的砖家。你能看到，资金早就要跑了。", "parent": "4614382678705143", "t": "1615781940"}, {"uid": "3370388762", "mid": "4615006836229721", "user_name": "业余宽客", "text": "黑黑兄弟资金早跑了之后竟然不跌反涨，你说神奇不神奇？", "parent": "4614986418356413", "t": "1615786800"}, {"uid": "6303477277", "mid": "4614696142898866", "user_name": "中观投资", "text": "你确定有报复性消费？", "parent": "4614382678705143", "t": "1615712760"}, {"uid": "3370388762", "mid": "4615006942137750", "user_name": "业余宽客", "text": "确定，不过中国除外，因为没发钱", "parent": "4614696142898866", "t": "1615786860"}, {"uid": "5876325815", "mid": "4614569903000546", "user_name": "在杭州的爱国的英语的微博", "text": "3月1⃣3⃣日", "parent": "4614382678705143", "t": "1615682640"}, {"uid": "3370388762", "mid": "4615007004263886", "user_name": "业余宽客", "text": "nan", "parent": "4614569903000546", "t": "1615786860"}, {"uid": "3234400130", "mid": "4614385194241154", "user_name": "ps修图帮主", "text": "支持优秀博主！加油", "parent": "4614382678705143", "t": "1615638600"}, {"uid": "3607659813", "mid": "4614383881159547", "user_name": "SanYa_胡国栋", "text": "晚上好", "parent": "4614382678705143", "t": "16156