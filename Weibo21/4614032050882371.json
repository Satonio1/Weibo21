[{"uid": "3047848725", "mid": "4614032050882371", "user_name": "南京那点小事", "text": "#南京# 经常被堵在机场高速正方大道匝道的同志们，好消息来了，匝道双向双道拓宽扩建啦！随着机场高速上下匝道免费，机场高速成为联络南京主城与江宁南部的高速快捷通道，但最后一个单道上下的免费匝道正方大道却是堵不堪言，路堵心更堵。尤其早晚高峰期从高速大老远堵至匝道，尽管交管部门后来又不得不治表临时限制高峰时段大货车通行，依旧改观甚微。一方面长道拥堵，真是不得不见缝插针，影响安全。大老远的高速右侧两股到匝道一股道汇聚，结果便是愈堵愈远。2、车多路远堵，心急赶点上下班，难免发生碰擦事故那就堵得更惨。3、严重影响高速车道及应急车道正常运转。现拟将秣陵半苜蓿叶形互通原位改造为全互通，同时将原半苜蓿单匝道改为双匝道，通行能力大幅提高。", "parent": null, "t": "1615554420"}, {"uid": "6429474010", "mid": "4614036312293810", "user_name": "风中摇曳的紫玫瑰", "text": "真是太给力了", "parent": "4614032050882371", "t": "1615555440"}, {"uid": "6075998768", "mid": "4614036312031336", "user_name": "心不安鼓", "text": "非常不错哦", "parent": "4614032050882371", "t": "1615555440"}, {"uid": "2198676901", "mid": "4614036271138018", "user_name": "Tksjgjl", "text": "谢谢分享", "parent": "4614032050882371", "t": "1615555380"}, {"uid": "3012284480", "mid": "4614036266159038", "user_name": "赘泊坷暇倔", "text": "确实便利了", "parent": "4614032050882371", "t": "1615555380"}, {"uid": "7324009024", "mid": "4614042963940387", "user_name": "潑墨為書YUKI", "text": "nan", "parent": "4614032050882371", "t": "1615557000"}, {"uid": "6046705538", "mid": "4614037013007638", "user_name": "深哥厉凡", "text": "是个好消息", "parent": "4614032050882371", "t": "1615555560"}, {"uid": "2232942812", "mid": "4614036865419975", "user_name": "青春李英子_783", "text": "真是太给力了", "parent": "4614032050882371", "t": "1615555560"}, {"uid": "3921255174", "mid": "4614036525945304", "user_name": "尚凝儿万岁", "text": "非常不错哦", "parent": "4614032050882371", "t": "1615555440"}, {"uid": "7515798497", "mid": "4614036513361328", "user_name": "苏州mars", "text": "经济在发展。时代在发展。一切多要跟上", "parent": "4614032050882371", "t": "1615555440"}, {"uid": "2134258351", "mid": "4614036496845793", "user_name": "快乐de士多碑梨", "text": "确实便利了", "parent": "4614032050882371", "t": "1615555440"}, {"uid": "2332091811", "mid": "4614036475347350", "user_name": "opasuigws", "text": "交通更方便了，点赞！", "parent": "4614032050882371", "t": "1615555440"}, {"uid": "1685