[{"uid": "3255691897", "mid": "4616112203105374", "user_name": "彭火火要火", "text": "【#腾讯阿里字节跳动等11家企业被约谈#】据“网信中国”18日公布，近日，国家互联网信息办公室、指导各地网信部门、机关加强对语音社交软件和涉“深度伪造”技术的互联网新技术新应用安全评估工作。\n\n针对近期未履行安全评估程序的语音社交软件和涉“深度伪造”技术的应用，国家互联网信息办公室、指导北京、天津、上海、浙江、广东等地方网信部门、机关依法约谈映客、小米、快手、字节跳动、鲸准数服、云账户、喜马拉雅、阿里巴巴、网易云音乐、腾讯、去演等11家企业\n\n督促其按照相关法律法规及政策要求，认真开展安全评估，完善风险防控机制和措施，并对安全评估中发现的安全隐患及时采取有效整改措施，切实履行企业信息内容安全主体责任。", "parent": null, "t": "1616050320"}, {"uid": "2257175873", "mid": "4616253622716731", "user_name": "热门微博", "text": "恭喜你的博文进入推荐流曝光~期待你创作更多优质内容，解锁更多奖励！", "parent": "4616112203105374", "t": "1616084040"}, {"uid": "5533667669", "mid": "4616123192182204", "user_name": "佳辛1118", "text": "转发微博", "parent": "4616112203105374", "t": "1616052960"}, {"uid": "7044425308", "mid": "4616123155221776", "user_name": "墨琴仙", "text": "转发微博", "parent": "4616112203105374", "t": "1616052960"}, {"uid": "7463276980", "mid": "4616122898583883", "user_name": "我们不是101", "text": "转发微博", "parent": "4616112203105374", "t": "1616052900"}, {"uid": "5978767633", "mid": "4616122845105120", "user_name": "蝶海花偌", "text": "转发微博", "parent": "4616112203105374", "t": "1616052900"}, {"uid": "5342071530", "mid": "4616122840125629", "user_name": "风风An森森", "text": "转发微博", "parent": "4616112203105374", "t": "1616052900"}, {"uid": "1450013254", "mid": "4616122835668250", "user_name": "Catalysis", "text": "转发微博", "parent": "4616112203105374", "t": "1616052900"}, {"uid": "2661224401", "mid": "4616122819153741", "user_name": "xlconfig", "text": "转发微博", "parent": "4616112203105374", "t": "1616052900"}, {"uid": "6781362213", "mid": "4616122818891295", "user_name": "明明就9962", "text": "转发微博", "parent": "4616112203105374", "t": "1616052900"}, {"uid": "1223785954", "mid": "4616122811287442", "user_name": "天边的这一抹云彩", "text": "转发微博", "parent": "4616112203105374", "t": "1616052900"}, {"uid": "5776434398", "mid": "4616122806829849", "user_name": "高岛塞宅", "text": "转发微博", "parent": "4616112203105374", "t": "1616052900"}, {"uid": "2750644380", "mi