[{"uid": "3176022652", "mid": "4613528641865729", "user_name": "遵义消防", "text": "#火焰蓝守护平安##遵义消防守护你#\n【一工人被钢筋刺穿 仁怀茅台消防紧急救援】\n     2021年3月10日17时20分，仁怀市茅台消防救援站接到报警称，仁怀市茅台镇上坪村新华组一工地有一工人肚子被插入钢筋，情况危急，茅台消防救援站立即出动1车7人赶赴现场进行救援。茅台消防救援站于17时55分到达现场，救援人员随即查看现场并设置警戒。在救援人员的齐心协力下，成功于18时16分切断钢筋救出被困人员，并移交医护人员。经询问知情人得知，现场被困工人是在距地面约3米高的（旋挖钻机）设备上作业，但因设备突然晃动，最终导致设备上的工人掉入圆坑中，坑周围的一根钢筋刺穿工人腹部。目前，具体事故原因正在进一步调查中。\n@遵义消防 提醒:防护加警惕保安全，无知加大意必危险。@贵州消防 @中国消防 L遵义消防的微博视频", "parent": null, "t": "1615434360"}, {"uid": "6664291776", "mid": "4613674251329538", "user_name": "考研政治押题王", "text": "行尸走肉", "parent": "4613528641865729", "t": "1615469100"}, {"uid": "5418028854", "mid": "4613629524051602", "user_name": "见千眼开", "text": "一定要注意安全", "parent": "4613528641865729", "t": "1615458420"}, {"uid": "5781195142", "mid": "4613626368889208", "user_name": "治不好哒神经病", "text": "一定要注意安全！", "parent": "4613528641865729", "t": "1615457700"}, {"uid": "2724454673", "mid": "4613626256951437", "user_name": "L李小群", "text": "太危险了，必须谨慎小心！", "parent": "4613528641865729", "t": "1615457640"}, {"uid": "1724391815", "mid": "4613625942114454", "user_name": "撕葱______", "text": "太可怜了", "parent": "4613528641865729", "t": "1615457580"}, {"uid": "7130911948", "mid": "4613625833326141", "user_name": "不过风雅颂", "text": "啊，一定要安全啊，要好好的(✪▽✪)", "parent": "4613528641865729", "t": "1615457520"}, {"uid": "7009455428", "mid": "4613625547588533", "user_name": "来抱我吗82994", "text": "不错不错不错不错", "parent": "4613528641865729", "t": "1615457460"}, {"uid": "2075951555", "mid": "4613625413633562", "user_name": "罐蚊", "text": "一定要注重安全意识！！", "parent": "4613528641865729", "t": "1615457460"}, {"uid": "5951657164", "mid": "4613625245075195", "user_name": "甜美爱莉花", "text": "看着都心疼，好难过", "parent": "4613528641865729", "t": "1615457400"}, {"uid": "5609215772", "mid": "4613624766664415", "user_name": "逸明九君", "text": "注意安全", "parent": "4613528641865729", "t": "1615457280"}, {"uid": "7363412357", "mid": "4613624565861749", "user_name": "小年糕wi", "text": "加强安全防护措施呢", "parent": "