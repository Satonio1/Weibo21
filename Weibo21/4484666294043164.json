[{"uid": "2788094952", "mid": "4484666294043164", "user_name": "军法缘", "text": "【增强生物国防意识】有消息称，美国的10艘医疗军舰已经开进纽约港，每艘船上有1000张病床，上面拥有所有的医疗抢救设施，每艘船相当于一个雷神山医院。可人家一下子就来了十艘，是典型的人类生物战争前兆。 2上海·万源城丨朗郡", "parent": null, "t": "1584711180"}, {"uid": "2869514164", "mid": "4484689635031019", "user_name": "三楼岚清", "text": "全球有正规大型专用医院船的只有美中俄三国，其中美军2艘，“仁慈”和“安慰”，中国1艘，“和平方舟”，俄罗斯4艘，包括“鄂毕河”、“叶尼塞河”、“斯维尔河”和“额尔齐斯河”。美国剩余的8艘从你的嘴里生产出来的？", "parent": "4484666294043164", "t": "1584716760"}, {"uid": "2685097383", "mid": "4484898356300001", "user_name": "永恒的长生天", "text": "谣言太多。幸亏有你", "parent": "4484689635031019", "t": "1584766500"}, {"uid": "5102232486", "mid": "4484669028056399", "user_name": "影视剪辑官", "text": "人之所以痛苦，在于追求错误的东西。", "parent": "4484666294043164", "t": "1584711840"}, {"uid": "2339186412", "mid": "4484668340938438", "user_name": "Sun皮匠", "text": "朕已阅", "parent": "4484666294043164", "t": "1584711660"}, {"uid": "6272988791", "mid": "4484666449003263", "user_name": "Korea-美少女", "text": "沙发~", "parent": "4484666294043164", "t": "1584711240"}, {"uid": "1350286032", "mid": "4484702608058978", "user_name": "第三天---", "text": "近日谣言很多，其中包括一个叫张某旗的人发帖“美国的10艘医疗军舰已经开进纽约港，每艘船上有1000张病床……”  真相：全球有正规大型专用医院船的只有美中俄三国，其中美军2艘，“仁慈”和“安慰”，中国1艘，“和平方舟”，俄罗斯4艘，包括“鄂毕河”、“叶尼塞河”、“斯维尔河”和“额尔齐斯河”", "parent": "4484666294043164", "t": "1584719880"}, {"uid": "2838554614", "mid": "4485333486100839", "user_name": "钱包给我呀", "text": "你真能吹", "parent": "4484666294043164", "t": "1584870240"}, {"uid": "1700720717", "mid": "4484961652164008", "user_name": "哎咛啊咛哎", "text": "没脑子", "parent": "4484666294043164", "t": "1584781620"}, {"uid": "1197764494", "mid": "4484740424064448", "user_name": "明静如台", "text": "美狗脸疼吗？", "parent": "4484666294043164", "t": "1584728880"}, {"uid": "2714812843", "mid": "4484669288450798", "user_name": "请问2006", "text": "就两艘，吹呢", "parent": "4484666294043164", "t": "1584711900"}, {"uid": "3168963240", "mid": "4484696505741594", "user_name": "凡夫与风语", "text": "二进制，没毛病", "parent": "4484669288450798", "t": "1584718380"