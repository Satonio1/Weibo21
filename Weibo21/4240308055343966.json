[{"uid": "5992595787", "mid": "4240308055343966", "user_name": "培训师_霖峰", "text": "郑州大学路长江路加油站爆炸了？真的假的？就在附近住，没有听见啊@赵逸rabbit 2郑州·第三大街 L主持人-船长哥哥的秒拍视频", "parent": null, "t": "1526451660"}, {"uid": "5880888132", "mid": "4240315215278060", "user_name": "茶里茶气的你", "text": "屁，是自燃的汽车", "parent": "4240308055343966", "t": "1526453340"}, {"uid": "2591407265", "mid": "4240432797914180", "user_name": "弗雷尔二哈爪子", "text": "回复@拉登是我枪杀的:爆炸个屁，加油站前边一栋楼着火了而已", "parent": "4240350597722255", "t": "1526481420"}, {"uid": "5880888132", "mid": "4240350862698054", "user_name": "茶里茶气的你", "text": "我在那边工作过，还在这里偷偷给我的小摩托加过油", "parent": "4240315215278060", "t": "1526461860"}, {"uid": "1856091862", "mid": "4240350597722255", "user_name": "拉登是我枪杀的", "text": "回复@老子叫上天:我就在大桥石化对面住，不是加油站就好，但愿没人受伤", "parent": "4240315215278060", "t": "1526461800"}, {"uid": "5880888132", "mid": "4240344025739471", "user_name": "茶里茶气的你", "text": "回复@拉登是我枪杀的:我知道是大桥石化，但是真的不是大桥石化石化爆炸，是货车爆炸在距离加油站两百米的位置", "parent": "4240317778043100", "t": "1526460240"}, {"uid": "1856091862", "mid": "4240317778043100", "user_name": "拉登是我枪杀的", "text": "确定？我朋友圈疯传，我就在这住，俩朋友发给我，说加油站爆炸，那加油站就是那个大桥石化了……特意来微博搜一搜还真有关于这事得", "parent": "4240315215278060", "t": "1526454000"}, {"uid": "1786832285", "mid": "4240590146699858", "user_name": "佛_心", "text": "严惩造谣视频发布者", "parent": "4240308055343966", "t": "1526518920"}, {"uid": "2375429371", "mid": "4240329174079148", "user_name": "_我叫红领巾_", "text": "是旁边酒店失火，不是加油站爆炸", "parent": "4240308055343966", "t": "1526456700"}, {"uid": "2947877663", "mid": "4240327513026498", "user_name": "砸开了", "text": "赶紧抓住这个造谣的女的。你怎么不说郑州市官员爆炸可？", "parent": "4240308055343966", "t": "1526456280"}, {"uid": "1634401402", "mid": "4240595607702537", "user_name": "我叫刘实", "text": "看你是没蹲过拘留@公安部打四黑除四害", "parent": "4240308055343966", "t": "1526520240"}, {"uid": "5505624579", "mid": "4240366456145116", "user_name": "做个好梦cc", "text": "明明是酒店着火", "parent": "4240308055343966", "t": "1526465580"}, {"uid": "5136355217", "mid": "4240557578371774", "user_name": "JR__迷妹", "text": "如果真爆炸，她还会拿着手机搁哪拍视频，不给她炸稀碎也半身不遂。", "parent": "4240308055343966", "t": "1526511120"}, {"uid": "5574895652", "mid": "4240321707312965", "user_name": "呆猫一只3280627", "text": "造谣吧，加油站爆炸，没有明火？没有黑烟？", "parent": "4240308055343966", "t": "1526454900"}, {"uid": "3842807964", "mid": "4240318829905766", "user_name": "铁塔大哥亲", "text": "别瞎说好么", "parent": "4240308055343966", "t": "1526454240"}, {"uid": "6080864319", "mid": "4247576858903498", "user_name": "小小木兮", "text": "加油站爆炸你还在那？嫌活的腻了吧", "parent": "4240308055343966", "t": "1528184700"}, {"uid": "5245152493", "mid": "4241789655022857", "user_name": "打伞的鱼刺", "text": "真倒霉，以后不要乱发了，被举报了", "parent": "4240308055343966", "t": "1526804880"}, {"uid": "2078129877", "mid": "4241783199802402", "user_name": "Sponge晓", "text": "停业的酒店，当时我就在现场。什么加油站爆炸，看热闹不闲事多。", "parent": "4240308055343966", "t": "1526803380"}, {"uid": "2000153447", "mid": "4241390147451213", "user_name": "从前寒暄", "text": "大兄弟，造谣啊", "parent": "4240308055343966", "t": "1526709660"}, {"uid": "5776472286", "mid": "4241358346654390", "user_name": "有什么更好的昵称推荐", "text": "别造谣 谢谢", "parent": "4240308055343966", "t": "1526702040"}, {"uid": "5831946115", "mid": "4240983275326561", "user_name": "鱼泡泡199008", "text": "是空置装修的酒店着火了，哪儿哪儿就爆炸了，都是谣言", "parent": "4240308055343966", "t": "1526612640"}, {"uid": "5928005569", "mid": "4240706660209160", "user_name": "特一安田_", "text": "别造谣了，爆炸的话你还在呀，只是失火了    而已！", "parent": "4240308055343966", "t": "1526546700"}, {"uid": "6234509065", "mid": "4241737058844028", "user_name": "青涩愁断网", "text": "造谣是要遭轮的", "parent": "4240308055343966", "t": "1526792340"}, {"uid": "5480242380", "mid": "4240770875768814", "user_name": "吴艳丽", "text": "怎么可能呢  嗨嗨", "parent": "4240308055343966", "t": "1526562000"}, {"uid": "1873419995", "mid": "4241455066840955", "user_name": "福潤元極", "text": "转发微博", "parent": "4240308055343966", "t": "1526725140"}, {"uid": "6541407612", "mid": "4241123503133893", "user_name": "用户6541407612", "text": "转发微博", "parent": "42403080553439