[{"uid": "1740006833", "mid": "4613870763118025", "user_name": "我不是谦哥儿", "text": "#中国AI虚拟模特取代高价外模# 中国有很多金发碧眼长腿细腰的外籍模特，他们大多是以俄罗斯、乌克兰为的，因为机票便宜，签证好办，不过他们大多三个月签证到期就要离开了。\n\n曾有位乌克兰男模说，来中国工作两三个月，回去就能买下乌克兰一套房。\n\n据我了解，有的外贸商家拼死拼活，一个月可能还没有一个模特赚的多。未来如果中国的这项AI虚拟模特技术推广开来，应该会帮商家省下不少钱\nL央视网的微博视频", "parent": null, "t": "1615515960"}, {"uid": "3547147520", "mid": "4613871242052233", "user_name": "伯爵红茶加奶半糖", "text": "不太理解为啥要用外模，其实我更喜欢看亚洲人试衣的，毕竟人种不同身材差别很大，看外模试衣选衣服不准。", "parent": "4613870763118025", "t": "1615516080"}, {"uid": "1644139187", "mid": "4614473237136051", "user_name": "庄舄越呤", "text": "……上面写着外贸商家", "parent": "4613871242052233", "t": "1615659600"}, {"uid": "7388902743", "mid": "4614251329358560", "user_name": "_愛因思劍B_", "text": "回复@努力建设厚-厚的墙:哇噻，模特行业人口红利也吃没了", "parent": "4613876979337379", "t": "1615606680"}, {"uid": "5071420702", "mid": "4614127336817143", "user_name": "镜子里有最帅的人", "text": "外模便宜也好拿捏", "parent": "4613871242052233", "t": "1615577100"}, {"uid": "2617721905", "mid": "4613940459606117", "user_name": "ylyker123", "text": "没错 ！！！", "parent": "4613871242052233", "t": "1615532580"}, {"uid": "1708928195", "mid": "4613917688468628", "user_name": "小懒懒懒岚", "text": "便宜，敬业，好看。", "parent": "4613871242052233", "t": "1615527120"}, {"uid": "5849669579", "mid": "4613901369739508", "user_name": "Echo提早进入老年生活", "text": " 国内那些模特也好看，但总有种网红脸的廉价感。", "parent": "4613871242052233", "t": "1615523220"}, {"uid": "3547147520", "mid": "4613880616064234", "user_name": "伯爵红茶加奶半糖", "text": "回复@Ispenggeda:嗯知道啊，就是顺便说说现在电商上有些名气的牌子的衣服模特基本都是外模为主了。", "parent": "4613878141683175", "t": "1615518300"}, {"uid": "5651275325", "mid": "4613878141683175", "user_name": "Ispenggeda", "text": "外贸订单 看清楚 又不是卖给中国人", "parent": "4613871242052233", "t": "1615517700"}, {"uid": "3547147520", "mid": "4613877831306952", "user_name": "伯爵红茶加奶半糖", "text": "回复@物种观察员:哈哈哈好吧，非常make sense了", "parent": "4613876970162043", "t": "1615517640"}, {"uid": "1660132982", "mid": "4613877809288366", "user_name": "梓潼_普通的起搏细胞_", "text": "真的希望模特能用正常身材的人...tb看到卖“大码”女装的，模特还是瘦子，就离谱", "parent": "4613871242052233", "t": "1615517640"}, {"uid": "2308428820", "mid": "4613876979337379", "user_name": "努力建设厚-厚的墙", "text": "外籍模特比国内模特便宜，然后腿长脸立体的，拍出来效果特别好。之前做服装电商的朋友就喜欢乌克兰模特，价格比国内便宜的多，身材比例好，又敬业事少，还放的开，后期修图也更简单", "parent": "4613871242052233", "t": "1615517400"}, {"uid": "1866517417", "mid": "4613876970162043", "user_name": "物种观察员", "text": "回复@伯爵红茶加奶半糖:害～我起初也质疑过，但是后来明白了老板们就是觉得偌大的网购群体必然不缺买衣服的，真正退的还是少数，只要头图能吸引人点进来看就能提高购买率", "parent": "4613876216496349", "t": "1615517400"}, {"uid": "3547147520", "mid": "4613876216496349", "user_name": "伯爵红茶加奶半糖", "text": "回复@极乐小夜曲:……啊那这个就超纲了", "parent": "4613875720522650", "t": "1615517220"}, {"uid": "1777155840", "mid": "4613875720522650", "user_name": "极乐小夜曲", "text": "有钱玩洋妞玩你不玩？", "parent": "4613871242052233", "t": "1615517100"}, {"uid": "3547147520", "mid": "4613875528109791", "user_name": "伯爵红茶加奶半糖", "text": "回复@物种观察员:但是终极要义不是要把衣服卖掉吗……修好了好看了但是大家买回来一试觉得还是瞎，然后又退回去，这……", "parent": "4613874705239594", "t": "1615517100"}, {"uid": "1866517417", "mid": "4613874705239594", "user_name": "物种观察员", "text": "电商从业者表示主要为了拍照好看，外模头小比例好修图方便，而且外模比中模便宜", "parent": "4613871242052233", "t": "1615516860"}, {"uid": "1941601661", "mid": "4613874667491558", "user_name": "冯小姗", "text": "对 希望用中国模特", "parent": "4613871242052233", "t": "1615516860"}, {"uid": "5844051799", "mid": "4613874634982022", "user_name": "斑马菲菲与犀牛厚厚", "text": "他们穿衣服好看", "parent": "4613871242052233", "t": "1615516860"}, {"uid": "3547147520", "mid": "4613873455859797", "user_name": "伯爵红茶加奶半糖", "text": "回复@F_猫丢了:嗯，以前瑞丽之类的平面模特就很好，跟着选衣服不太出错。都外模以后，我坦白说退单率还挺高。", "parent": "4613872633255112", "t": "1615516560"}, {"uid": "3547147520", "mid": "4613873141813608", "user_name": "伯爵红茶加奶半糖", "text": "回复@墨夜麟:没错没错，就是老外审美型的中国模特，也是很迷。", "parent": "4613872881767251", "t": "1615516500"}, {"uid": "2209134093", "mid": "4613872881767251", "user_name": "墨夜麟", "text": "我不能理解的是很多国风服装用的模特都小眼睛单眼皮长眼型，这生活中中国人也不都长这样啊，这分明就是外国人眼中的中国人。", "parent": "4613871242052233", "t": "1615516440"}, {"uid": "2625183677", "mid": "4613872633255112", "user_name": "F_猫丢了", "text": "+1，买了就是卖家秀和买家秀", "parent": "4613871242052233", "t": "1615516380"}, {"uid": "1568277965", "mid": "4613872407549425", "user_name": "清桐大人", "text": "身材比例的优势", "parent": "4613871242052233", "t": "1615516320"}, {"uid": "2366176530", "mid": "4613872357213721", "user_name": "阿猹吃瓜不吃花", "text": "我猜是有的衣服设计和质感很差需要外国模特的身材修饰bug+外国脸提高档次？", "parent": "4613871242052233", "t": "1615516320"}, {"uid": "7279734375", "mid": "4613871971860660", "user_name": "__那样的过去", "text": "崇洋媚外呗", "parent": "4613871242052233", "t": "1615516260"}, {"uid": "1170112027", "mid": "4613871766081142", "user_name": "ageton", "text": "可能为了高档感吧", "parent": "4613871242052233", "t": "1615516200"}, {"uid": "1813724835", "mid": "4613871514423707", "user_name": "Shieru-K", "text": "大概是看脸看身材的比看衣服的多", "parent": "4613871242052233", "t": "1615516140"}, {"uid": "5597362645", "mid": "4613871867006449", "user_name": "工科的文科同学", "text": "不懂就问：花销这么大，为什么没有刺激乌克兰国内的模特供应量大幅增加呢？按说价格杠杆会发挥作用的呀", "parent": "4613870763118025", "t": "1615516200"}, {"uid": "5849669579", "mid": "4613901499760835", "user_name": "Echo提早进入老年生活", "text": " 模特靠天吃饭（身高和脸），你要怎么量产？", "parent": "4613871867006449", "t": "1615523280"}, {"uid": "2308428820", "mid": "4613877368887537", "user_name": "努力建设厚-厚的墙", "text": "回复@工科的文科同学:也可能是信息差", "parent": "4613873389273757", "t": "1615517520"}, {"uid": "3578875354", "mid": "4613875007490470", "user_name": "Dei55555", "text": "模特培训至少得半年才能赶得上到现在淘宝商家要求的工作量", "parent": "4613871867006449", "t": "1615516980"}, {"uid": "5597362645", "mid": "4613873389273757", "user_name": "工科的文科同学", "text": "回复@我不是谦哥儿:培养周期、培养费用可能都限制了。但假如是中国提供模特给外国，然后3个月可以买套房的话，我相信几年时间，模特价格就会被打下来---这可能就是这几十年中国发展迅猛的原因之一吧", "parent": "4613872600748818", "t": "1615516560"}, {"uid": "1740006833", "mid": "4613872600748818", "user_name": "我不是谦哥儿", "text": "模特有准入门槛的啊~", "parent": "4613871867006449", "t": "1615516380"}, {"uid": "2313757225", "mid": "4613880582245570", "user_name": "我是焕能不是贱人", "text": "问个当外模经纪人的朋友，他说没听说过ai模特的事，现在疫情早就停了签证，现在在国内基本上都是去年下半年的从白俄和塞尔维亚过来的。工作三个月就回国买房的没听说过，工作两三年买房的就有。", "parent": "4613870763118025", "t": "1615518300"}, {"uid": "1160651911", "mid": "4613891990489759", "user_name": "花落成荫", "text": "二三个月买房的可能还从事了别的业务", "parent": "4613880582245570", "t": "1615521000"}, {"uid": "1482219750", "mid": "4613871761099263", "user_name": "佳佳女巫的俩猪宝", "text": "外国人都爱来中国挣钱", "parent": "4613870763118025", "t": "1615516200"}, {"uid": "1779056755", "mid": "4613894221858538", "user_name": "法兰西斯菜", "text": "这些亏钱的商家也真是活该亏，搞个模特能提高销量？品质跟价格才是正路，你不亏还能有谁", "parent": "4613870763118025", "t": "1615521540"}, {"uid": "1819891274", "mid": "4613883849606864", "user_name": "守道林壑", "text": "欧洲脸就这么高级", "parent": "4613870763118025", "t": "1615519080"}, {"uid": "2040525633", "mid": "4614086732547264", "user_name": "热烈XX爱好者", "text": "外贸要用", "parent": "4613870763118025", "t": "1615567440"}, {"uid": "1736790842", "mid": "4613893512760818", "user_name": "海_KAI", "text": "乌克兰表演人员大部分都不是很想长做，毕竟谁喜欢背井离乡呢，能做完一年都少见了。上一年更惨，有些人因疫情和经济停滞而提早回去乌克兰了，谁知道后来恢复这么快，但是想再入境中国工作就没那么容易了。还是因为疫情绊脚，一是签证不好拿了，二是雇主听说演员才刚从外国进来也不敢聘用。", "parent": "4613870763118025", "t": "1615521360"}, {"uid": "1655847500", "mid": "4613891440510306", "user_name": "优l质", "text": "那个穿特效服的画面也太侮辱亚裔了……还是自己侮辱自己………", "parent": "4613870763118025", "t": "1615520880"}, {"uid": "2929092370", "mid": "4613878967963011", "user_name": "妙音音777", "text": "那他们不就没工作了", "parent": "4613870763118025", "t": "1615517880"}, {"uid": "1444343027", "mid": "4614025797175591", "user_name": "19启航", "text": "转发微博", "parent": "4613870763118025", "t": "1615552920"}, {"uid": "6694706373", "mid": "4613916242477981", "user_name": "腊肉萝卜丝", "text": "转发微博", "parent": "4613870763118025", "t": "1615526760"}, {"uid": "1659110043", "mid": "4613904343237689", "user_name": "羊咩咩_2015", "text": "转发微博", "parent": "4613870763118025", "t": "1615523940"}, {"uid": "2152698571", "mid": "4613895790790524", "user_name": "OlegBaymini", "text": "转发微博", "parent": 