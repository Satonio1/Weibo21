[{"uid": "1067344672", "mid": "4134257703819865", "user_name": "西安生活印记", "text": "#西安身边事##西安终于下雨啦# 西安市公安局通知：紧急通知：刚刚市里开了全市防汛紧急电视电话会议，今天晚上开始至明天有特大暴雨（有可能有1998年那么大洪水），家有住危房和河边低洼地带、山洪地质灾害易发区旁的亲戚赶快通知，今、明两晚不要到不安全的地段居住，在低处开门面的赶快抓紧时间将商品转移至高处；今天白天大家储备好用水，大暴雨后自来水可能会浊二、三天！车辆不要停在低洼地带。一定要关注财产、人身安全！希望大家无论是在家里还是出行千万注意安全，尤其导购人员，外出骑车经过低洼区域请绕行，消息准确，请大家务必重视，在各部门群里转发一下！下雨啦🌧️ L秒拍视频", "parent": null, "t": "1501167300"}, {"uid": "1052381253", "mid": "4134392139727012", "user_name": "michealbai", "text": "你这公众号能不能不要这么没脑子！！", "parent": "4134257703819865", "t": "1501199340"}, {"uid": "6099774185", "mid": "4134334920672638", "user_name": "张美兔的鱼", "text": "谣言", "parent": "4134257703819865", "t": "1501185720"}, {"uid": "2099663115", "mid": "4134304655212108", "user_name": "轩辕溟天", "text": "造谣  举报了", "parent": "4134257703819865", "t": "1501178460"}, {"uid": "6274288670", "mid": "4134273507976068", "user_name": "LVJIABO", "text": "考验西安城市下水道的时候到了，好不容易下雨了", "parent": "4134257703819865", "t": "1501171020"}, {"uid": "6023429287", "mid": "4134450268710321", "user_name": "风儿把我吹的好任性", "text": "未经核实发这个出来你是找骂？就这样还做公众号，已举报", "parent": "4134257703819865", "t": "1501213200"}, {"uid": "2618518975", "mid": "4134405842591205", "user_name": "李全博客視界", "text": "举报，谣言", "parent": "4134257703819865", "t": "1501202580"}, {"uid": "5666239662", "mid": "4134391913400811", "user_name": "y1种思念灬唯一", "text": "不是谣言么", "parent": "4134257703819865", "t": "1501199280"}, {"uid": "2612692710", "mid": "4134390856283856", "user_name": "非我莫笑", "text": "真的？", "parent": "4134257703819865", "t": "1501199040"}, {"uid": "1960993857", "mid": "4134387781987575", "user_name": "叫什么名字好呢_55", "text": "骗人的", "parent": "4134257703819865", "t": "1501198320"}, {"uid": "6131094248", "mid": "4134281309935065", "user_name": "无敌大英豪", "text": "确定是西安的吗？前几天是延安市发的，今天变成西安了吗？", "parent": "4134257703819865", "t": "1501172880"}, {"uid": "2322678745", "mid": "4134607051973387", "user_name": "姗姗一袅楚宫腰", "text": "每年一到雨季朋友圈就出来了", "parent": "4134257703819865", "t": "